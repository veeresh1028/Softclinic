        function switchLanguage() {
            var languageSpan = document.querySelector('.language');

            if (languageSpan.textContent.trim() === 'EN') {
                localStorage.setItem("lang", "EN");
            }
            else if (languageSpan.textContent.trim() === 'إلغاء') {
                localStorage.setItem("lang", "AR");
            }
            else {
                localStorage.setItem("lang", "EN");
            }

            setLanguage();
        }

        function setLanguage() {
            var lang = localStorage.getItem("lang");
            var languageSpan = document.querySelector('.language');
            var flagImage = document.querySelector('.country');
            var htmlElement = document.querySelector('html');

            if (lang != null) {
                if (lang === "EN") {
                    languageSpan.textContent = 'إلغاء';
                    flagImage.src = '/assets/images/ar_flag.jpg';
                    htmlElement.setAttribute('dir', 'ltr');

                    $('body').removeClass('rtl');
                    $('body').addClass('ltr');
                }
                else {
                    languageSpan.textContent = 'ENG';
                    flagImage.src = '/assets/images/us_flag.jpg'; 
                    htmlElement.setAttribute('dir', 'rtl');

                    $('body').addClass('rtl');
                    $('body').removeClass('ltr');
                }
            }
            else {
                languageSpan.textContent = 'إلغاء';
                flagImage.src = '/assets/images/ar_flag.jpg';
                htmlElement.setAttribute('dir', 'ltr');

                $('body').removeClass('rtl');
                $('body').addClass('ltr');
                localStorage.setItem("lang", "EN");
            }

            getLanguage();
        }

        function getLanguage() {
            var language = localStorage.getItem("lang");
            var request = new XMLHttpRequest();
            // Instantiating the request object
            request.open("GET", "/assets/lang/" + language + ".json");
            // Defining event listener for readystatechange event
            request.onreadystatechange = function () {
                // Check if the request is compete and was successful
                if (this.readyState === 4 && this.status === 200) {
                    var data = JSON.parse(this.responseText);

                    Object.keys(data).forEach(function (key) {
                        var elements = document.querySelectorAll("[data-lang-key='" + key + "']");
                        Array.from(elements).forEach(function (elem) {
                            elem.textContent = data[key];
                        });
                    });
                }
            };
            // Sending the request to the server
            request.send();
        }

    </script>
