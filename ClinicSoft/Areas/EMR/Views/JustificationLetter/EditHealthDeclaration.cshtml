    //#region Update  Button Click
        $('#btn_Update_declaration').on('click', function (e) {
             e.preventDefault();
            if ($("#form_EditScreening").valid()) {
                $('#btn_Update_declaration').removeClass("btn btn-sm btn-success");
                $('#btn_Update_declaration').addClass("btn btn-sm btn-success btn-loaders btn-icon");
                $('#btn_Update_declaration').html("Updating the Record....");
                var uchd_checkbox_ = "";
                $('.custom-control-input:checked').each(function (index) {
                    uchd_checkbox_ = uchd_checkbox_ + ',' + $(this).val();
                });
                var _dataInsert = {
                    "chd_Id": @Model.chd_Id,
                    "chd_Prob1": $("#uchd_Prob1").val(),
                    "chd_Prob2": $("#uchd_Prob2").val(),
                    "chd_Prob3": $("#uchd_Prob3").val(),
                    "chd_Prob4": $("#uchd_Prob4").val(),
                    "chd_Prob5": $("#uchd_Prob5").val(),
                    "chd_checkbox": uchd_checkbox_,
                    "pa_pain": $("#upa_pain").val(),
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateHealthDeclaration", "JustificationLetter", new { area = "EMR" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_declaration').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_declaration').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_declaration').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> GetHealth Declaration Updated Successfully!", "success");
                                Timeout();
                                GetInitialTherapyScreening();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. GetHealth Declaration Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_declaration').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_declaration').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_declaration').html("Save Changes");
                        Timeout();
                    }
                });
            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion Update  Button Click


    //#region Delete Click
        $('#btn_Delete_declaration').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_declaration').removeClass("btn btn-sm btn-success");
            $('#btn_Delete_declaration').addClass("btn btn-sm btn-success btn-loaders btn-icon");
            $('#btn_Delete_declaration').html("Deleting the Record....");

                $.ajax({
                    url: '@Url.Action("DeleteHealthDeclaration", "JustificationLetter", new { area = "EMR" })?chd_Id=' + @Model.chd_Id,
                    type: "POST",
                    dataType: 'JSON',
                    success: function (data) {
                        $('#btn_Delete_declaration').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_declaration').addClass("btn btn-sm btn-warning");
                        $('#btn_Delete_declaration').html("Delete");
                        if (data.isAuthorized) {
                            if (data.success) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> GetHealth Declaration Deleted Successfully!", "success");
                                clearControls();
                                GetHealthDeclaration();
                                Timeout();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_declaration').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_declaration').addClass("btn btn-sm btn-Delete");
                        $('#btn_Delete_declaration').html("Delete");
                        Timeout();
                    }
                })
        });
    //#endregion Delete Click


