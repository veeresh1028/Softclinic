                                    <label class="form-label">Post-cricoid region</label>
                                    <label class="text-left">@Model.et_postc&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Posterior pharyngeal wall</label>
                                    <label class="text-left">@Model.et_postp&nbsp;</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-2">
                            <div class="col-sm-12 col-md-6 col-lg-12">
                                <div class="expanel expanel-default">
                                    <div class="expanel-heading">
                                        <h3 class="expanel-title">Neck</h3>
                                    </div>
                                </div>
                            </div>
                        
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Parotids</label>
                                    <label class="text-left">@Model.et_paro&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Thyroid</label>
                                    <label class="text-left">@Model.et_thy&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Traches</label>
                                    <label class="text-left">@Model.et_trac&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Larynx</label>
                                    <label class="text-left">@Model.et_lary&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Lymph nodes</label>
                                    <label class="text-left">@Model.et_lymp&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-2">
                                <div class="form-group mb-3">
                                    <label class="form-label">Other</label>
                                    <label class="text-left">@Model.et_others&nbsp;</label>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group mb-3">
                                    <label class="form-label">Notes</label>
                                    <label class="text-left">@Model.et_notes&nbsp;</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center">
                    <img src="@Model.set_footer_image" />
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">

    function printDiv(divName) {
        var printContents = document.getElementById(divName).innerHTML;
        var originalContents = document.body.innerHTML;
        document.body.innerHTML = printContents;
        window.print();
        document.body.innerHTML = originalContents;
    }
</script>


