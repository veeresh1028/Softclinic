

@using BusinessEntities;
@model BusinessEntities.EMR.RevisionRequest
@{
    ViewBag.Title = "Print Botex Picture Men";
    Layout = "~/Areas/EMR/Views/Shared/_EMR_Layout.cshtml";
    EMRInfo emr = (EMRInfo)TempData["emr_data"];
    TempData.Keep("emr_data");
}
<div class="row mb-2 mt-2 text-center">
    <div class="col-md-12">
        <button id="btnPrint" type="button" onclick="printDiv('printDiv')" class="btn btn-primary d-print-none"><i class="si si-printer"></i> Print</button>
    </div>
</div>
<div class="row" id="printDiv">
    <div class="col-md-12">
        <div class="card overflow-hidden">
            <div class="card-body">
                <div class="text-center">
                    <img src="@Model.set_header_image" />
                </div>

                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-6">
                        <div>
                            <h4 class="page-title mb-0 text-primary">
                                Approval Revision Request Form
                            </h4>
                        </div>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-sm-12 col-md-6 col-lg-12">
                        <div class="row border ">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="row">
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Patient Name:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left text-capitalize">@emr.pat_name&nbsp;</label>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black"> Emirates ID:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                        <label class="form-label text-red text-left">@emr.pat_emirateid&nbsp;</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3 text-align-left"><label class="form-label text-black ">File No :&nbsp;</label></div>

                                    </div>

                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.pat_code&nbsp;</label>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black"> DOB&nbsp;</label></div>

                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.pat_dob&nbsp;</label>
                                    </div>

                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black"> Nationality:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.pat_nationality&nbsp;</label>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black">Gender&nbsp;</label></div>

                                    </div>

                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.pat_gender&nbsp;</label>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black">Doctor's Name	:&nbsp;</label></div>

                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.doc_name&nbsp;</label>
                                    </div>

                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3"><label class="form-label text-black">Date&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <label class="form-label text-red text-left">@emr.app_fdate&nbsp;</label>
                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-sm-12 col-sm-6 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label text-black text-align-left"> Name&nbsp; </label>
                            <label class="form-label text-red text-left text-capitalize">@emr.pat_name&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-12 col-sm-6 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label text-black text-align-left"> MRN&nbsp; </label>
                            <label class="form-label text-red text-left text-capitalize">@emr.pat_code&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label text-black">Date&nbsp;</label>
                            <div class="input-group">

                                <label class="form-label text-red text-left text-capitalize">@Model.carr_date.ToString("DD-MMMM-YYYY")&nbsp;</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-2 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label">Card No.</label>
                            <label class="form-label text-red text-left text-capitalize">@Model.carr_e1&nbsp;</label>
                        </div>
                    </div>


                    <div class="col-sm-12 col-md-6 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label text-black">Requested Date&nbsp;</label>
                            <div class="input-group">
                                <label class="form-label text-red text-left text-capitalize">@Model.carr_e3&nbsp;</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label text-black">Auth Expiry&nbsp;</label>
                            <div class="input-group">
                                <label class="form-label text-red text-left text-capitalize">@Model.carr_e4&nbsp;</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-2 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label">Present Auth No</label>
                            <label class="form-label text-red text-left text-capitalize">@Model.carr_e2&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-2 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label">Approved Quantity</label>
                            <label class="form-label text-red text-left text-capitalize">@Model.carr_e5&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-2 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label">Utilized sessions</label>
                            <label class="form-label text-red text-left text-capitalize">@Model.carr_e6&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-2 col-lg-2">
                        <div class="form-group mb-3">
                            <label class="form-label">Required Quantity</label>
                            <label class="form-label text-red text-left text-capitalize">@Model.carr_e7&nbsp;</label>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-4"></div>
                    <div class="col-sm-12 col-md-6 col-lg-12">
                        <div class="form-group mb-3">
                            <label class="form-label text-black">Reason for revision&nbsp;</label>
                            <div class="custom-controls d-lg-flex d-md-flex flex-row justify-content-left">
                                <label class="custom-control custom-radio  ms-2">
                                    <input type="radio" class="custom-control-input" name="ucarr_checkbox" value="Authorization Expired" id="1">
                                    <span class="custom-control-label">Authorization Expired</span>
                                </label>
                                <label class="custom-control custom-radio  ms-2">
                                    <input type="radio" class="custom-control-input" name="ucarr_checkbox" value="Card Expired" id="2">
                                    <span class="custom-control-label">Card Expired</span>
                                </label>
                                <label class="custom-control custom-radio  ms-2">
                                    <input type="radio" class="custom-control-input" name="ucarr_checkbox" value="Patient Discontinued and Resumed" id="2">
                                    <span class="custom-control-label">Patient Discontinued and Resumed</span>
                                </label>
                                <label class="custom-control custom-radio  ms-2 me-2">
                                    <input type="radio" class="custom-control-input" name="ucarr_checkbox" value="Other" id="2">
                                    <span class="custom-control-label">Other</span>
                                </label>
                                <label class="form-label text-red text-left text-capitalize">@Model.carr_other&nbsp;</label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11 col-md-12 col-lg-4">
                            <label class="form-label mb-2">
                                Doctor Name & Licence
                            </label>
                            <span class="text-capitalize text-left text-red">@emr.doc_name&nbsp;</span>
                            <span class="text-capitalize text-left text-red">@emr.emp_license&nbsp;</span>

                        </div>
                        <div class="col-sm-11 col-md-12 col-lg-4">
                            <label class="form-label mb-2">
                                Signature/Stamp
                            </label>
                            <img alt="stamp" src="@emr.doc_sign" id="doc_sign" class="avatar avatar-xl bsquare" />

                        </div>
                        <div class="col-sm-11 col-md-12 col-lg-4">
                            <label class="form-label mb-2">  Date & Time     </label>
                            <span class="text-capitalize text-left text-red">@Model.carr_date_created&nbsp;</span>

                        </div>

                    </div>
                </div>
                <div class="text-center">
                    <img src="@Model.set_footer_image" />
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
     var pd_1_ = "@Model.carr_checkbox";
        if (pd_1_ !== "") {
            var upd_1_ = $("input[name='ucarr_checkbox'][value='" + "@Model.carr_checkbox" + "']").attr("id");
            $("#" + upd_1_).attr('checked', 'checked');
            $("#" + upd_1_).prop("checked", true);
        }
    function printDiv(divName) {
        var printContents = document.getElementById(divName).innerHTML;
        var originalContents = document.body.innerHTML;
        document.body.innerHTML = printContents;
        window.print();
        document.body.innerHTML = originalContents;
    }
</script>


