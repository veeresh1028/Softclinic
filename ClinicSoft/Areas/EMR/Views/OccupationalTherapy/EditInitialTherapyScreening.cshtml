                        <input type="checkbox" class="custom-control-input" name="name101" value="235" id="235">
                        <span class="custom-control-label">Regular nursery	</span>
                    </label>
                    <label class="custom-control custom-checkbox  ms-2">
                        <input type="checkbox" class="custom-control-input" name="name101" value="240" id="240">
                        <span class="custom-control-label">Others</span>
                    </label>

                </div>
            </div>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3">
            <div class="form-group mb-3">
                <label class="form-label text-black">
                    Others
                    &nbsp;
                </label>
                @Html.TextAreaFor(m => m.cot_txt17, new { @class = "form-control", @placeholder = "Enter Comments", maxlength = "500", @id = "ucot_txt17" })
            </div>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3">
            <div class="form-group mb-3">
                <label class="form-label text-black">
                    Assessment Findings & Recommendations<span class="text-danger">*</span>
                    &nbsp;
                </label>
                @Html.TextAreaFor(m => m.cot_txt18, new { @class = "form-control", @placeholder = "Enter Comments", maxlength = "500", @id = "ucot_txt18" })
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-11 col-md-12 col-lg-4">
            <label class="form-label mb-2">
                Doctor Name & Licence
            </label>
            <span class="text-capitalize text-left text-red">@emr.doc_name&nbsp;</span>
            <span class="text-capitalize text-left text-red">@emr.emp_license&nbsp;</span>

        </div>
        <div class="col-sm-11 col-md-12 col-lg-4">
            <label class="form-label mb-2">
                Signature/Stamp
            </label>
            <img alt="stamp" src="@emr.doc_sign" id="doc_sign" class="avatar avatar-xl bsquare" />

        </div>
        <div class="col-sm-11 col-md-12 col-lg-4">
            <label class="form-label mb-2">  Date & Time     </label>
            <span class="text-capitalize text-left text-red">@Model.cot_date_created&nbsp;</span>

        </div>

    </div>

    <div class="row mt-2">
        <div class="col-sm-12 col-md-6 col-lg-12 text-center">
            <div class="btn-list btn-animation">
                <button type="submit" class="btn btn-sm btn-warning" id="btn_Update_Screening">Save Changes</button>
                <button type="submit" class="btn btn-sm btn-outline-danger" id="btn_Delete_Screening">Delete</button>
                <button type="submit" class="btn btn-sm btn-outline-light" id="btn_Print_Screening" onclick="PrintInitialTherapyScreening(@Model.cot_Id)">
                    <i class="fa fa-print">&nbsp;&nbsp;</i>Print
                </button>
            </div>
        </div>
    </div>

</form>


<script>
    //#region Page Load
        $(function () {
            validate();
            if ("@emp_designation.ToString()" === "Super Administrator") {
                $("#btn_Update_Screening,#btn_Delete_Screening").show();
            } else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Update_Screening,#btn_Delete_Screening").show();

                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Update_Screening,#btn_Delete_Screening").show();
                    }
                    else {
                        $("#btn_Update_Screening,#btn_Delete_Screening").hide();
                    }
                }
            }
            $('#ucot_date').bootstrapdatepicker({
                format: "dd-MM-yyyy",
                viewMode: "date",
                todayHighlight: true,
                autoclose: true,
                multidate: false,
                multidateSeparator: "-"
            });

            $("#ucot_date").bootstrapdatepicker("setDate", moment(new Date("@Model.cot_date")).format("DD-MMMM-YYYY"));

            GetCheckedValues();

        });

    //#endregion Page Load

    //#region Get CheckedValues
    var GetCheckedValues = function () {

         var pd_1_ = "@Model.cot_checkbox";
        if (pd_1_ !== "") {
            var str = "@Model.cot_checkbox";
            var myarray = str.split(',');

            for (var i = 0; i < myarray.length; i++) {
                $('.custom-control-input').each(function () {
                    if (this.id == myarray[i]) {
                        $("#" + this.id).attr('checked', 'checked');
                        $("#" + this.id).prop("checked", true);

                    }
                });
            }
        }

    }
    //#endregion


     //#endregion Get CheckedValues
    //#region Update  Button Click
        $('#btn_Update_Screening').on('click', function (e) {
             e.preventDefault();
            if ($("#form_EditScreening").valid()) {
                $('#btn_Update_Screening').removeClass("btn btn-sm btn-success");
                $('#btn_Update_Screening').addClass("btn btn-sm btn-success btn-loaders btn-icon");
                $('#btn_Update_Screening').html("Updating the Record....");
                var cot_checkbox_ = "";
                $('.custom-control-input:checked').each(function (index) {
                    cot_checkbox_ = cot_checkbox_ + ',' + $(this).val();
                });
                var _dataInsert = {
                    "cot_Id": @Model.cot_Id,
                    "cot_appId": $("#cot_appId").val(),
                    "cot_txt_sib": $("#ucot_txt_sib").val(),
                    "cot_txt_inf": $("#ucot_txt_inf").val(),
                    "cot_txt1": $("#ucot_txt1").val(),
                    "cot_txt2": $("#ucot_txt2").val(),
                    "cot_txt3": $("#ucot_txt3").val(),
                    "cot_txt4": $("#ucot_txt4").val(),
                    "cot_txt5": $("#ucot_txt5").val(),
                    "cot_txt6": $("#ucot_txt6").val(),
                    "cot_txt7": $("#ucot_txt7").val(),
                    "cot_txt8": $("#ucot_txt8").val(),
                    "cot_txt9": $("#ucot_txt9").val(),
                    "cot_txt10": $("#ucot_txt10").val(),
                    "cot_txt11": $("#ucot_txt11").val(),
                    "cot_txt12": $("#ucot_txt12").val(),
                    "cot_txt13": $("#ucot_txt13").val(),
                    "cot_txt14": $("#ucot_txt14").val(),
                    "cot_txt15": $("#ucot_txt15").val(),
                    "cot_txt16": $("#ucot_txt16").val(),
                    "cot_txt17": $("#ucot_txt17").val(),
                    "cot_txt18": $("#ucot_txt18").val(),
                    "cot_date": $("#ucot_date").val(),
                    "cot_checkbox": cot_checkbox_,
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateInitialTherapyScreening", "OccupationalTherapy", new { area = "EMR" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_Screening').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Screening').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_Screening').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Initial Therapy Screening Updated Successfully!", "success");
                                Timeout();
                                GetInitialTherapyScreening();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. Initial Therapy Screening Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_Screening').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Screening').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_Screening').html("Save Changes");
                        Timeout();
                    }
                });
            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion Update  Button Click


    //#region Delete Click
        $('#btn_Delete_Screening').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_Screening').removeClass("btn btn-sm btn-success");
            $('#btn_Delete_Screening').addClass("btn btn-sm btn-success btn-loaders btn-icon");
            $('#btn_Delete_Screening').html("Deleting the Record....");

                $.ajax({
                    url: '@Url.Action("DeleteInitialTherapyScreening", "OccupationalTherapy", new { area = "EMR" })?cot_Id=' + @Model.cot_Id,
                    type: "POST",
                    dataType: 'JSON',
                    success: function (data) {
                        $('#btn_Delete_Screening').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_Screening').addClass("btn btn-sm btn-warning");
                        $('#btn_Delete_Screening').html("Delete");
                        if (data.isAuthorized) {
                            if (data.success) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Initial Therapy Screening Deleted Successfully!", "success");
                                clearControls();
                                GetInitialTherapyScreening();
                                Timeout();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_Screening').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_Screening').addClass("btn btn-sm btn-Delete");
                        $('#btn_Delete_Screening').html("Delete");
                        Timeout();
                    }
                })
        });
    //#endregion Delete Click

    //#region Validation Initialization
        var validate = function () {

            $("#form_EditScreening").validate({
                rules: {
                    ucot_Imp: {
                        required: true
                    },

                },
                messages: {
                    ucot_Imp: {
                        required: "Please Enter Clinical Impression !"
                    },

                },
                highlight: function (element) {
                    var elem = $(element);
                    $(element).parent().addClass('has-error');
                    error_timeout(elem.attr("id"));
                },
                unhighlight: function (element) {
                    var elem = $(element);
                    $(element).parent().removeClass('has-error');
                },
                errorElement: 'span',
                errorClass: 'text-danger fw-bold',
                errorPlacement: function (error, element) {
                    var elem = $(element);
                    if (element.parent('.input-group').length) {
                        error.insertAfter(element.parent());
                    } else {
                        error.insertAfter(element);
                    }
                }
            });


        }
    //#endregion Validation Initialization

    //#region Clear Controls
        var clearControls = function () {
            $(".form-control").val("");
            $(".custom-control-input").prop("checked", false);

        }
    //#endregion Clear Controls
</script>


