                    "pd_prolonged": $("input[name='pd_prolonged']:checked").val(),
                    "pd_gems": $("input[name='pd_gems']:checked").val(),
                    "pd_filling": $("input[name='pd_filling']:checked").val(),
                    "pd_w_medicaltreat": $("input[name='pd_w_medicaltreat']:checked").val(),
                    "pd_w_hospitalized": $("input[name='pd_w_hospitalized']:checked").val(),
                    "pd_w_medications": $("input[name='pd_w_medications']:checked").val(),
                    "pd_w_amenorrhoea": $("#pd_w_amenorrhoea").val(),
