                    "pd_teethpain": $("input[name='pd_teethpain']:checked").val(),
                    "pd_soresjumps": $("input[name='pd_soresjumps']:checked").val(),
                    "pd_headinjuries": $("input[name='pd_headinjuries']:checked").val(),
                    "pd_clicking": $("input[name='pd_clicking']:checked").val(),
