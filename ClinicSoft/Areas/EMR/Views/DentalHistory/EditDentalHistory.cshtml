<script>
    //#region Page Load
        $(function () {
           
            if ("@emp_designation.ToString()" === "Super Administrator") {
                $("#btn_Update_DentHis,#btn_Delete_DentHis").show();
            } else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Update_DentHis,#btn_Delete_DentHis").show();

                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Update_DentHis,#btn_Delete_DentHis").show();
                    }
                    else {
                        $("#btn_Update_DentHis,#btn_Delete_DentHis").hide();
                    }
                }
            }
            GetCheckedValues();
            $('#upd_last_dental_exam').bootstrapdatepicker({
                format: "dd-MM-yyyy",
                viewMode: "date",
                todayHighlight: true,
                autoclose: true,
                multidate: false,
                multidateSeparator: "-"
            });
            if ("@Model.pd_last_dental_exam" != "") {
