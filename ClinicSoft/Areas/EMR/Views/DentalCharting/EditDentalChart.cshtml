                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_DentalChart').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_DentalChart').addClass("btn btn-sm btn-Delete");
                        $('#btn_Delete_DentalChart').html("Delete");
                        Timeout();
                    }
                })
        });
