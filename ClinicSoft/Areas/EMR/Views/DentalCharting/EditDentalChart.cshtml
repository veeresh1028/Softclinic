@using System.Configuration;
@using System.Linq;
@using System.Security.Claims;
@using BusinessEntities;
@model BusinessEntities.EMR.DentalCharting
@{
    var claims = ClaimsPrincipal.Current.Identities.First().Claims.ToList();
    var emp_designation = claims.Where(c => c.Type == ClaimTypes.Role).Select(c => c.Value).SingleOrDefault();
    EMRInfo emr = (EMRInfo)TempData["emr_data"];
    TempData.Keep("emr_data");
}
<form id="form_EditDentChart" class="needs-validation" name="DentalChart">

    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.cdcId)
    @Html.HiddenFor(m => m.cdc_appId)
    @Html.HiddenFor(m => m.cdcId, new { @id = "ucdcId", @value = "@Model.cdcId" })

    <div class="row mb-4">
        <div class="col-sm-12 col-md-6 col-lg-12">
            <div>
                <h4 class="page-title mb-0 text-primary text-center">
                    Dental Charting
                </h4>
            </div>
        </div>
    </div>
    <div class="row col-sm-12 col-md-12 col-lg-12 mb-4 mt-4">
        <!-- add Dental Chart icons -->
