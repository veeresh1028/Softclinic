    });
    //#endregion Page Load


    //#region Bind Existing Data
    var toothIds = [];
    var modifiedId = "";
    var ToothIds = function () {
        var elements = document.getElementsByClassName("avatar");
        for (var i = 1; i <= 32; i++) {
            var element = elements[i];
            if (element.tagName === "IMG") { // Check if the element is an image
                var id = element.getAttribute("id"); // Get the id attribute
                modifiedId = id.replace('u', '');
                toothIds.push(parseInt(modifiedId, 10));
            }
        }
    }
    var PerioChartDetails = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetPerioChartDetails", "DentalCharting", new { area = "EMR" })?appId=" + "@emr.appId",
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var _data = response;
                        if (_data.length > 0) {
                            for (var i = 0; i < _data.length; i++) {
                                var k= response[i].pchd_tooth;
                                if (toothIds.includes(response[i].pchd_tooth)) {
                                    $("#u" + k).val(response[i].pchdId);
                                    $("#u" + k + "plaque").val(response[i].pchd_plaque);
                                    $("#u" + k + "mobklkty").val(response[i].pchd_mobklkty);
                                    $("#u" + k + "boneloss").val(response[i].pchd_boneloss);
                                    $("#u" + k + "mgd").val(response[i].pchd_mgd);
                                    $("#u" + k + "calt1").val(response[i].pchd_calt1);
                                    $("#u" + k + "calt2").val(response[i].pchd_calt2);
                                    $("#u" + k + "calt3").val(response[i].pchd_calt3);
                                    $("#u" + k + "gmt1").val(response[i].pchd_gmt1);
                                    $("#u" + k + "gmt2").val(response[i].pchd_gmt2);
                                    $("#u" + k + "gmt3").val(response[i].pchd_gmt3);
                                    $("#u" + k + "pdt1").val(response[i].pchd_pdt1);
                                    $("#u" + k + "pdt2").val(response[i].pchd_pdt2);
                                    $("#u" + k + "pdt3").val(response[i].pchd_pdt3);
                                    $("#u" + k + "t1").val(response[i].pchd_t1);
                                    $("#u" + k + "t2").val(response[i].pchd_t2);
                                    $("#u" + k + "t3").val(response[i].pchd_t3);
                                    $("#u" + k + "t4").val(response[i].pchd_t4);
                                    $("#u" + k + "t5").val(response[i].pchd_t5);
                                    $("#u" + k + "t6").val(response[i].pchd_t6);
                                    $("#u" + k + "pdb1").val(response[i].pchd_pdb1);
                                    $("#u" + k + "pdb2").val(response[i].pchd_pdb2);
                                    $("#u" + k + "pdb3").val(response[i].pchd_pdb3);
                                    $("#u" + k + "gmb1").val(response[i].pchd_gmb1);
                                    $("#u" + k + "gmb2").val(response[i].pchd_gmb2);
                                    $("#u" + k + "gmb3").val(response[i].pchd_gmb3);
                                    $("#u" + k + "calb1").val(response[i].pchd_calb1);
                                    $("#u" + k + "calb2").val(response[i].pchd_calb2);
                                    $("#u" + k + "calb2").val(response[i].pchd_calb3);

                                    for (var l = 1; l <= 6; l++) {
                                        if ($("#u" + k + "t" + l).val() === "B") {
                                            $("#u" + k + "t" + l).css({
                                                color: "red",
                                                backgroundColor: "red"
                                            });
                                        } else if ($("#u" + k + "t" + l).val() === "S") {
                                            $("#u" + k + "t" + l).css({
                                                color: "orange",
                                                backgroundColor: "orange"
                                            });
                                        }
                                    }
                                }
                            }

                        }
                        else {

                        }
                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }

    //#endregion

        //#region AutoRow
        var AutoRow = function () {
            var numberRow = $("#unumberRow");

            for (var i = 1; i <= 16; i++) {
                var column = $("<div>").addClass("column").text(i);
                numberRow.append(column);
            }
            var numberRow1 = $("#unumberRow1");

            for (var i = 32; i > 16; i--) {
                var column1 = $("<div>").addClass("column").text(i);
                numberRow1.append(column1);
            }
        }

    //#endregion AutoRow
    function printDiv(divName) {
        var printContents = document.getElementById(divName).innerHTML;
        var originalContents = document.body.innerHTML;
        document.body.innerHTML = printContents;
        window.print();
        document.body.innerHTML = originalContents;
    }
