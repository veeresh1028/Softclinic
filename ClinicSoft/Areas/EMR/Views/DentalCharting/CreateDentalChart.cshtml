        $('#btn_Insert_DentalChart').on('click', function (e) {
            e.preventDefault();
            var isvalid = true;

            if (quill.getText().trim().length == 0) {
                $("#chartnote").attr("style", "border:1px solid red;");
                error_timeout_quil($("#chartnote"));
                isvalid = false;
            }

            /*if (isDrawing) {*/
            if (isvalid) {
                $('#btn_Insert_DentalChart').removeClass("btn btn-sm btn-success");
                $('#btn_Insert_DentalChart').addClass("btn btn-sm btn-success btn-loaders btn-icon");
                $('#btn_Insert_DentalChart').html("Saving the Record....");

                var canvas = $("#imageCanvas").get(0);
                var imgData = canvas.toDataURL();
                console.log(imgData);

                var chartname = "@emr.patId"+"_"+"DentalChart"+"_"+"@emr.appId";
                var _dateInsert = {
                    "cdc_appId": @emr.appId,
                    "cdc_patId": "@emr.patId",
                    "cdc_chartname": chartname,
                    "image": imgData,
                    "cdc_note": quill.root.innerHTML,
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "Post",
                    url: "@Url.Action("UploadDentalChart", "DentalCharting", new {area = "EMR" })",
                    dataType: 'JSON',
                    data: _dateInsert,
                    success: function (response) {
                        $('#btn_Insert_DentalChart').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Insert_DentalChart').addClass("btn btn-sm btn-success");
                        $('#btn_Insert_DentalChart').html("Add Dental Chart");
                        if (response.isSuccess) {
                            console.log(response);
                            if (response.isInserted) {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i>  Dental Charting Saved Successfully!", "success");
                            GetDentalChart();
                            Timeout();
                        }
                        else {

                            let html = "<div class='col-12 col-sm-12 col-md-6 d-flex justify-content-center'>" +
                                "<div class='alert alert-warning' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Warning.. Dental Charting Alredy Exists..</strong><br/>" +
                                "</div></div>";
                            $("#Alert").html(html);
                        }
                        }
                        else {
                        let html = "<div class='col-12 col-sm-12 col-md-6 d-flex justify-content-center'>" +
                        "<div class='alert alert-danger' role='alert'>" +
                        "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                        "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                                $.each(response.message, function (index, value) {
                            html += "<li><small>" + value + "</small><li>";
                            var elem = $("#" + index);
                            if (elem.hasClass("select2-hidden-accessible")) {
                                $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                            }
                            else {
                                $(elem).addClass(" is-invalid");
                            }

                            setTimeout(function () {
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                }
                                else {
                                    $(elem).removeClass("is-invalid");
                                }
                            }, 5000);
                        });
                        html += "</ul></div></div>";
                        $("#error").html(html);
                        }
                    },
                        error: function (xhr) {
                            $('#btn_Insert_DentalChart').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                            $('#btn_Insert_DentalChart').addClass("btn btn-sm btn-warning");
                            $('#btn_Insert_DentalChart').html("Add Dental Chart");
                        Timeout();
                    }
                });
            }
            //}
            //else {
            //    alert('Please Draw');
            //}
        });
