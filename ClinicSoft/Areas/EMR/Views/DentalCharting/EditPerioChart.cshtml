    //#region ToothFill
    var ButtonSelect = function (id) {
        var parts = id.split('_');
        var j = parts[parts.length - 1];
        var selectedColor = $("input[name='color']:checked").val();
        for (var i = 1; i <= 32; i++) {
            if (selectedToothId == i) {

                if (selectedColor === "red-dark") {
                    $("#u" + i + "t" + j).css('background-color', 'red');
                    $("#u" + i + "t" + j).css('color', 'red')
                    $("#u" + i + "t" + j).val("B");
                } else if (selectedColor === "danger") {
                    $("#u" + i + "t" + j).css('background-color', 'orange');
                    $("#u" + i + "t" + j).css('color', 'orange');
                    $("#u" + i + "t" + j).val("S");
                }
                bindtootthaction(i);
            }

        }

    }
    //#endregion

    //#region tempdata
    var toothExists = false;
    var bindtootthaction = function (i) {


        var foundIndex = -1;
        for (var j = 0; j < item_arr.length; j++) {
            if (item_arr[j].pchd_tooth === i) {
                toothExists = true;
                foundIndex = j;
                break;
            }
        }

        var newData = {
            "pchdId": 0,
            "pchd_tooth": i,
            "pchd_plaque": $("#u" + i + "plaque").val(),
            "pchd_mobility": $("#u" + i + "mobility").val(),
            "pchd_boneloss": $("#u" + i + "boneloss").val(),
            "pchd_mgd": $("#u" + i + "mgd").val(),
            "pchd_calt1": $("#u" + i + "calt1").val(),
            "pchd_calt2": $("#u" + i + "calt2").val(),
            "pchd_calt3": $("#u" + i + "calt3").val(),
            "pchd_gmt1": $("#u" + i + "gmt1").val(),
            "pchd_gmt2": $("#u" + i + "gmt2").val(),
            "pchd_gmt3": $("#u" + i + "gmt3").val(),
            "pchd_pdt1": $("#u" + i + "pdt1").val(),
            "pchd_pdt2": $("#u" + i + "pdt2").val(),
            "pchd_t1": $("#u" + i + "t1").val(),
            "pchd_t2": $("#u" + i + "t2").val(),
            "pchd_t3": $("#u" + i + "t3").val(),
            "pchd_t4": $("#u" + i + "t4").val(),
            "pchd_t5": $("#u" + i + "t5").val(),
            "pchd_t6": $("#u" + i + "t6").val(),
            "pchd_pdb1": $("#u" + i + "pdb1").val(),
            "pchd_pdb2": $("#u" + i + "pdb2").val(),
            "pchd_pdb3": $("#u" + i + "pdb3").val(),
            "pchd_gmb1": $("#u" + i + "gmb1").val(),
            "pchd_gmb2": $("#u" + i + "gmb2").val(),
            "pchd_gmb3": $("#u" + i + "gmb3").val(),
            "pchd_calb1": $("#u" + i + "calb1").val(),
            "pchd_calb2": $("#u" + i + "calb2").val(),
            "pchd_calb3": $("#u" + i + "calb3").val(),
        };

        if (toothExists && foundIndex !== -1) {
            item_arr.splice(foundIndex, 1);
            item_arr[foundIndex] = newData;

        } else {
            // Add the new data to the array
            item_arr.push(newData);
        }

    }
