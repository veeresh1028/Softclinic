@model BusinessEntities.EMR.MusculoSkeletal
@using BusinessEntities;
@using System.Linq;
@using System.Security.Claims;
@{
    var claims = ClaimsPrincipal.Current.Identities.First().Claims.ToList();
    var emp_designation = claims.Where(c => c.Type == ClaimTypes.Role).Select(c => c.Value).SingleOrDefault();
    EMRInfo emr = (EMRInfo)TempData["emr_data"];
    TempData.Keep("emr_data");
    ViewBag.Title = "Index";
    Layout = "~/Areas/EMR/Views/Shared/_EMR_Layout.cshtml";
}

<div class="row">
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-center">
            <div class="alert alert-danger alert-dismissible fade show" role="alert">
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                <strong>Error!&nbsp;</strong>@TempData["ErrorMessage"]
            </div>
        </div>
    }
    <div class="col-sm-12 col-md-12 col-lg-12">
        <div class="card">

            <div class="card-body">
                <div>
                    <form id="form_Musculo" autocomplete="off" name="MusculoSkeletal" ondrop="return false;" onpaste="return false;" oncut="return false;">
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(m => m.msId, new { @id = "umsId", @value = "@Model.msId" })
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12 mb-4 font-weight-semibold">
                                <span class='badge bg-danger'>Required</span>
                                <span class="text-info" style="font-size:smaller">Please Fill atlest One Entry</span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12 mb-4 ">
                                <div class="row">
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div>
                                            <h4 class="page-title mb-0 text-primary">
                                                Musculo Skeletal
                                            </h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Shoulder:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_1_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_1_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_1, new { @class = "form-control", @placeholder = "Enter Shoulder", @id = "ums_1" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Elbows:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_2_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_2_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_2, new { @class = "form-control", @placeholder = "Enter Elbows", @id = "ums_2" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Wrist & Finger:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_3_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_3_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_3, new { @class = "form-control", @placeholder = "Enter Wrist & Finger", @id = "ums_3" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Hip	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_4_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_4_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_4, new { @class = "form-control", @placeholder = "Enter Hip", @id = "ums_4" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Others:&nbsp;<span class="text-danger"> *</span></label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_5_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_5_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_5, new { @class = "form-control", @placeholder = "Enter Others", @id = "ums_5" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Medical DecisionMaking:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.ms_6_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ums_6_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.ms_6, new { @class = "form-control", @placeholder = "Enter Medical Decision Making", @id = "ums_6" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12 col-lg-12 text-center">
                                        <div class="btn-list btn-animation">
                                            <button type="submit" class="btn btn-success" id="btn_Insert_Musculo">Add</button>
                                            <button type="button" class="btn btn-outline-dark" id="btn_Reset_Musculo">Reset</button>
                                            <button type="submit" class="btn btn-sm btn-outline-warning" id="btn_Update_Musculo">Save Changes</button>
                                            <button type="submit" class="btn btn-sm btn-outline-danger" id="btn_Delete_Musculo">Delete</button>
                                            <button type="submit" class="btn btn-sm btn-outline-light" id="btn_Print" value="Print" onclick="ViewMusculo(this.msId)" style="display:none">
                                                <i class="fa fa-print">&nbsp;&nbsp;</i>Print
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div>
                    <h4 class="page-title mb-0 text-primary">
                        Previous History
                    </h4>
                </div>
                <div class="table-responsive">
                    <table id="tbl_PreMusculoSkeletal" class="table table-striped table-bordered text-nowrap" style="width: 100%; cursor: pointer;">
                        <thead>
                            <tr style="color: #fff !important; background-color: #808080; border-color: #3C457D; ">
                                <th class="border-bottom-0 font-weight-semibold text-white">#</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Date</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Doctor</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Heart Rate</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Rhythm</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">JVP</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Heart Sounds</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Murmur</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Peripheral Pulses</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="ms_modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" id="size">
        <div class="modal-content" id="ms_body">
        </div>
    </div>
</div>


@*@section myScripts{*@
<script>
    //#region Page Load
        $(function () {
            BindDataTableP(null);
            GetPreviousMusculoSkeletal();
            GetMusculoSkeletal();
            validate();
            BindSelect();
            if ("@emp_designation.ToString()" === "Super Administrator") {
                $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();
            } else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();
                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();
                    }
                    else {
                        $("#btn_Insert_Musculo,#btn_Reset_Musculo").hide();
                    }
                }
            }

        });
    //#endregion Page Load

    //#region Reset
        $('#btn_Reset_Musculo').on('click', function (e) {
            e.preventDefault();
            clearControls();
        });
    //#endregion Reset

    //#region Load MusculoSkeletal
        var GetMusculoSkeletal = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllMusculoSkeletal", "MusculoSkeletal", new { area = "EMR" })?appId=" + "@emr.appId",
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var _data = response;
                        console.log(response);
                        if (_data.length > 0) {
                            console.log(response[0].msId);
                            $('#umsId').val(response[0].msId);
                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                $("#btn_Update_Musculo,#btn_Delete_Musculo").show();
                            } else {
                                if ("@emr.set_emr_lock" == "NoLock") {
                                    $("#btn_Update_Musculo,#btn_Delete_Musculo").show();

                                }
                                else {
                                   if ("@emr.app_ae_date" >= "@emr.today") {
                                        $("#btn_Update_Musculo,#btn_Delete_Musculo").show();
                                    }
                                    else {
                                        $("#btn_Update_Musculo,#btn_Delete_Musculo").hide();
                                    }
                                }
                            }
                            $("#btn_Insert_Musculo,#btn_Reset_Musculo").hide();

                        }
                        else {
                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();
                            } else {
                                if ("@emr.set_emr_lock" == "NoLock") {
                                    $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();

                                }
                                else {
                                    if ("@emr.app_ae_date" >= "@emr.today") {
                                        $("#btn_Insert_Musculo,#btn_Reset_Musculo").show();
                                    }
                                    else {
                                        $("#btn_Insert_Musculo,#btn_Reset_Musculo").hide();
                                    }
                                }
                            }
                            $("#btn_Update_Musculo,#btn_Delete_Musculo").hide();

                        }
                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Load MusculoSkeletal

    //#region Load PreviousMusculoSkeletal
        var GetPreviousMusculoSkeletal = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllPreMusculoSkeletal", "MusculoSkeletal", new { area = "EMR" })?appId=" + "@emr.appId" + "&patId=" + @emr.patId,
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var table;
                        if ($.fn.dataTable.isDataTable('#tbl_PreMusculoSkeletal')) {
                            table = $('#tbl_PreMusculoSkeletal').DataTable();
                            table.clear();
                            table.rows.add(response).draw();
                        }
                        else {
                            console.log(response);
                            BindDataTableP(response);
                        }
                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Load PreviousMusculoSkeletal

    //#region Previous Bind Datatable
        var BindDataTableP = function (response) {
            var table = $("#tbl_PreMusculoSkeletal").DataTable({
                fixedHeader: {
                    header: true,
                    footer: true
                },
                processing: true,
                "deferRender": true,
                "pageLength": 100,
                "retrieve": true,
                lengthChange: true,
                "aaData": response,
                "aoColumns": [
                    {
                        "mData": "msId",
                        "orderable": false,
                        "searchable": false,
                        "className": "text-center",
                        "render": function (data, type, row, meta) {
                            let _index = (meta.row) + 1;
                            if (_index >= 10) {
                                return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>" + _index + "</a>";
                            }
                            else {
                                return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>0" + _index + "</a>";
                            }
                        }
                    },

                    {
                        "mData": "app_fdate",
                        "className": "text-left",
                        "render": function (data) {
                            return moment(data).format('DD-MM-YYYY')
                        }
                    },
                    {
                        "mData": "doctor_name",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "ms_1_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "ms_2_type",
                        "className": 'text-justify text-wrap'

                    },
                    {
                        "mData": "ms_3_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "ms_4_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "ms_5_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "ms_6_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "msId",
                        "className": 'btn-view text-center',
                        "orderable": false,
                        "searchable": false,
                        "render": function (msId, type, full, meta) {
                            var Action = '';
                            //Copy Data
                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                Action += '<a id=btn_Copy_Musculo  onclick="CopyPrevious(' + msId + ')"><i class="fa fa-lg fa-copy" ></i></a>';
                                }
                                else {
                                if ("@emr.set_emr_lock" == "NoLock") {
                                    Action += '<a id=btn_Copy_Musculo  onclick="CopyPrevious(' + msId + ')"><i class="fa fa-lg fa-copy" ></i></a>';


                                    }
                                    else {
                                        if ("@emr.app_ae_date" >= "@emr.today") {
                                            Action += '<a id=btn_Copy_Musculo  onclick="CopyPrevious(' + msId + ')"><i class="fa fa-lg fa-copy" ></i></a>';
                                        }
                                        else {
                                            Action += ''
                                            }
                                    }
                                }

                                return Action;
                            }
                    },
                ],
                order: [[0, 'asc']],
                language: {
                    searchPlaceholder: 'Search...',
                    sSearch: '<span class="text-primary font-weight-semi-bold">Search By Code/Tittle</span>',
                    lengthMenu: '_MENU_',
                    scrollX: "100%",
                },
                scrollX: true,
                scrollY: 500,
                scrollCollapse: true,
                buttons: [
                    {
                        extend: 'excel',
                        title: 'Previous Musculo Skeletal',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2, 3, 4, 5, 6, 7, 8]
                        }
                    },
                    {
                        extend: 'pdf',
                        title: 'Previous Musculo Skeletal',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2, 3, 4, 5, 6, 7, 8]
                        }
                    },
                    {
                        extend: 'csv',
                        title: 'Previous Musculo Skeletal',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2, 3, 4, 5, 6, 7, 8]
                        }
                    },
                    {
                        extend: 'print',
                        title: 'Previous Musculo Skeletal',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2, 3, 4, 5, 6, 7, 8]
                        }
                    }
                ]
            });
            table.buttons().container().appendTo('#tbl_PreMusculoSkeletal_wrapper .col-md-6:eq(0)');
        };
    //#endregion Previous Bind Datatable

    //#region Copy Previous  Musculo
        function CopyPrevious(msId) {
            var x = $("#btn_Copy_Musculo");
                $.ajax({
                type: "GET",
                    url: "@Url.Action("CopyMusculoSkeletal", "MusculoSkeletal", new { area = "EMR" })?msId=" + msId,
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var _data = response;
                        console.log(response);
                        if (_data.length > 0) {
                            console.log(response[0].ms_3_type);
                            $('#ums_1').val(response[0].ms_1);
                            $('#ums_2').val(response[0].ms_2);
                            $('#ums_3').val(response[0].ms_3);
                            $('#ums_4').val(response[0].ms_4);
                            $('#ums_5').val(response[0].ms_5);
                            $('#ums_6').val(response[0].ms_6);
                            $('#ums_1_type').val(response[0].ms_1_type);
                            $('#ums_2_type').val(response[0].ms_2_type);
                            $('#ums_3_type').val(response[0].ms_3_type);
                            $('#ums_4_type').val(response[0].ms_4_type);
                            $('#ums_5_type').val(response[0].ms_5_type);
                            $('#ums_6_type').val(response[0].ms_6_type);
                            BindSelect();
                        }

                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Copy Previous  Musculo

    //#region Add  Click
        $('#btn_Insert_Musculo').on('click', function (e) {
            e.preventDefault();
            if ($("#form_Musculo").valid()) {
                $('#btn_Insert_Musculo').removeClass("btn btn-success");
                $('#btn_Insert_Musculo').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Insert_Musculo').html("Saving the Record....");
                var _dateInsert = {
                    "msId": 0,
                    "ms_appId": @emr.appId,
                    "ms_1": $("#ums_1").val(),
                    "ms_2": $("#ums_2").val(),
                    "ms_3": $("#ums_3").val(),
                    "ms_4": $("#ums_4").val(),
                    "ms_5": $("#ums_5").val(),
                    "ms_6": $("#ums_6").val(),
                    "ms_1_type": $("#ums_1_type").val(),
                    "ms_2_type": $("#ums_2_type").val(),
                    "ms_3_type": $("#ums_3_type").val(),
                    "ms_4_type": $("#ums_4_type").val(),
                    "ms_5_type": $("#ums_5_type").val(),
                    "ms_6_type": $("#ums_6_type").val(),
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                console.log(_dateInsert);
                $.ajax({
                    type: "Post",
                    url: "@Url.Action("InsertMusculoSkeletal", "MusculoSkeletal", new {area = "EMR" })",
                    dataType: 'JSON',
                    data: _dateInsert,
                    success: function (data) {
                        $('#btn_Insert_Musculo').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Insert_Musculo').addClass("btn btn-success");
                        $('#btn_Insert_Musculo').html("Add");
                        if (data.isInserted) {
                            console.log(data);
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i>  Musculo Skeletal Saved Successfully!", "success");
                            GetMusculoSkeletal();
                            Timeout();
                        }
                        else {

                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-warning' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Warning.. Musculo Skeletal Alredy Exists..</strong><br/>" +
                                "</div></div>";
                            $("#Alert").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Insert_Musculo').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Insert_Musculo').addClass("btn btn-warning");
                        $('#btn_Insert_Musculo').html("Add");
                        Timeout();
                    }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion Add Click

    //#region Update  Click
        $('#btn_Update_Musculo').on('click', function (e) {
            e.preventDefault();
            if ($("#form_Musculo").valid()) {
