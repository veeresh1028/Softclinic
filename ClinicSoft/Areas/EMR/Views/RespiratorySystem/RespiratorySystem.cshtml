@model BusinessEntities.EMR.RespiratorySystem
@using BusinessEntities;
@using System.Linq;
@using System.Security.Claims;
@{
    var claims = ClaimsPrincipal.Current.Identities.First().Claims.ToList();
    var emp_designation = claims.Where(c => c.Type == ClaimTypes.Role).Select(c => c.Value).SingleOrDefault();
    EMRInfo emr = (EMRInfo)TempData["emr_data"];
    TempData.Keep("emr_data");
    ViewBag.Title = "Index";
    Layout = "~/Areas/EMR/Views/Shared/_EMR_Layout.cshtml";
}

<div class="row">
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-center">
            <div class="alert alert-danger alert-dismissible fade show" role="alert">
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                <strong>Error!&nbsp;</strong>@TempData["ErrorMessage"]
            </div>
        </div>
    }
    <div class="col-sm-12 col-md-12 col-lg-12">
        <div class="card">

            <div class="card-body">
                <div>
                    <form id="form_RespSystem" autocomplete="off" name="respiratorysystem" ondrop="return false;" onpaste="return false;" oncut="return false;">
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(m => m.resId, new { @id = "uresId", @value = "@Model.resId" })
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12 mb-4 font-weight-semibold">
                                <span class='badge bg-danger'>Required</span>
                                <span class="text-info" style="font-size:smaller">Please Fill atlest One Entry</span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12 mb-4 ">
                                <div class="row">
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div>
                                            <h4 class="page-title mb-0 text-primary">
                                                Respiratory System
                                            </h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Chest:&nbsp;<span class="text-danger"> *</span></label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_1_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_1_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_1, new { @class = "form-control", @placeholder = "Enter Chest", @id = "ures_1" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Dyspoenia:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_2_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_2_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_2, new { @class = "form-control", @placeholder = "Enter Dyspoenia", @id = "ures_2" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Dilated Veins	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_3_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_3_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_3, new { @class = "form-control", @placeholder = "Enter Dilated Veins", @id = "ures_3" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Chest Expansion	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_4_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_4_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_4, new { @class = "form-control", @placeholder = "Enter Chest Expansion", @id = "ures_4" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Chest Percussion	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_5_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_5_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_5, new { @class = "form-control", @placeholder = "Enter Chest Percussion", @id = "ures_5" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Breath Sounds	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_6_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_6_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_6, new { @class = "form-control", @placeholder = "Enter Breath Sounds", @id = "ures_6" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Vocal Resonance	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_7_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_7_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_7, new { @class = "form-control", @placeholder = "Enter Vocal Resonance", @id = "ures_7" })
                                        </div>
                                    </div>
                                    <div class="col-sm-1 col-md-1 col-lg-1">
                                        <div class="form-group mb-3"><label class="form-label text-black text-align-left"> Adventitious Sounds	:&nbsp;</label></div>
                                    </div>
                                    <div class="col-sm-2 col-md-2 col-lg-2">
                                        <div class="form-group mb-3">
                                            @Html.DropDownListFor(m => m.res_8_type, new List<SelectListItem>
                                             {
                                                new SelectListItem{ Text="Normal", Value = "Normal" },
                                                new SelectListItem{ Text="Abnormal", Value = "Abnormal" },
                                                new SelectListItem{ Text="Not-Indicated", Value = "Not-Indicated" },
                                             }, "Select Type", new { @class = "form-select select2", @id = "ures_8_type" })
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <div class="form-group mb-3">
                                            @Html.TextBoxFor(m => m.res_8, new { @class = "form-control", @placeholder = "Enter Adventitious Sounds", @id = "ures_8" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12 col-lg-12 text-center">
                                        <div class="btn-list btn-animation">
                                            <button type="submit" class="btn btn-success" id="btn_Insert_RespSystem">Add</button>
                                            <button type="button" class="btn btn-outline-dark" id="btn_Reset_RespSystem">Reset</button>
                                            <button type="submit" class="btn btn-sm btn-outline-warning" id="btn_Update_RespSystem">Save Changes</button>
                                            <button type="submit" class="btn btn-sm btn-outline-danger" id="btn_Delete_RespSystem">Delete</button>
                                            <button type="submit" class="btn btn-sm btn-outline-light" id="btn_Print" value="Print" onclick="ViewRespSystem(this.resId)" style="display:none">
                                                <i class="fa fa-print">&nbsp;&nbsp;</i>Print
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div>
                    <h4 class="page-title mb-0 text-primary">
                        Previous History
                    </h4>
                </div>
                <div class="table-responsive">
                    <table id="tbl_PreRespiratorySystem" class="table table-striped table-bordered text-nowrap" style="width: 100%; cursor: pointer;">
                        <thead>
                            <tr style="color: #fff !important; background-color: #808080; border-color: #3C457D; ">
                                <th class="border-bottom-0 font-weight-semibold text-white">#</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Date</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Doctor</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Chest</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Dyspoenia</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Dilated Veins</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Chest Expansion</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Chest Percussion</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Breath Sounds</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Vocal Resonance</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Adventitious Sounds</th>
                                <th class="border-bottom-0 font-weight-semibold text-white">Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="res_modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" id="size">
        <div class="modal-content" id="res_body">
        </div>
    </div>
</div>

@*@section myScripts{*@

    <script>
    //#region Page Load
        $(function () {
            GetPreviousRespiratorySystem();
            GetRespiratorySystem();
            BindSelect();
            if ("@emp_designation.ToString()" === "Super Administrator") {
               $("#btn_Insert_RespSystem").show();
                $("#btn_Reset_RespSystem").show();
            } else {
                if ("@emr.set_emr_lock" != "NoLock") {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Insert_RespSystem").show();
                        $("#btn_Reset_RespSystem").show();
                    }
                    else {
                        $("#btn_Insert_RespSystem").hide();
                        $("#btn_Reset_RespSystem").hide();
                    }
                }
                else {
                    $("#btn_Insert_RespSystem").show();
                    $("#btn_Reset_RespSystem").show();
                }
            }

        });
        //validate();
    //#endregion Page Load

    //#region Reset
        $('#btn_Reset_RespSystem').on('click', function (e) {
            e.preventDefault();
            clearControls();
        });
    //#endregion Reset


    //#region Load RespiratorySystem
        var GetRespiratorySystem = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllRespiratorySystem", "RespiratorySystem", new { area = "EMR" })?appId=" + "@emr.appId",
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var _data = response;
                        console.log(response);
                        if (_data.length > 0) {
                            console.log(response[0].resId);
                            $('#uresId').val(response[0].resId);
                            //hide the Copy Data
                            var oTable = $('#tbl_PreRespiratorySystem').dataTable();
                            oTable.$('#btn_Copy_RespSystem').hide();

                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                $("#btn_Update_RespSystem,#btn_Delete_RespSystem").show();
                            } else {
                                if ("@emr.set_emr_lock" != "NoLock") {
                                    if ("@emr.app_ae_date" >= "@emr.today") {
                                        $("#btn_Update_RespSystem,#btn_Delete_RespSystem").show();
                                    }
                                    else {
                                        $("#btn_Update_RespSystem,#btn_Delete_RespSystem").hide();
                                    }
                                }
                                else {
                                    $("#btn_Update_RespSystem,#btn_Delete_RespSystem").show();
                                }
                            }
                            $("#btn_Insert_RespSystem").hide();
                            $("#btn_Reset_RespSystem").hide();

                        }
                        else {
                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                $("#btn_Insert_RespSystem").show();
                                $("#btn_Reset_RespSystem").show();
                            } else {
                                if ("@emr.set_emr_lock" != "NoLock") {
                                    if ("@emr.app_ae_date" >= "@emr.today") {
                                        $("#btn_Insert_RespSystem").show();
                                        $("#btn_Reset_RespSystem").show();
                                    }
                                    else {
                                        $("#btn_Insert_RespSystem").hide();
                                        $("#btn_Reset_RespSystem").hide();
                                    }
                                }
                                else {
                                    $("#btn_Insert_RespSystem").show();
                                    $("#btn_Reset_RespSystem").show();
                                }
                            }
                            $("#btn_Update_RespSystem,#btn_Delete_RespSystem").hide();

                        }
                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Load RespiratorySystem

    //#region Load PreviousRespiratorySystem
        var GetPreviousRespiratorySystem = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllPreRespiratorySystem", "RespiratorySystem", new { area = "EMR" })?appId=" + "@emr.appId" + "&patId=" + @emr.patId,
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var table;
                        if ($.fn.dataTable.isDataTable('#tbl_PreRespiratorySystem')) {
                            table = $('#tbl_PreRespiratorySystem').DataTable();
                            table.clear();
                            table.rows.add(response).draw();
                        }
                        else {
                            console.log(response);
                            BindDataTableP(response);
                        }
                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Load PreviousRespiratorySystem

    //#region Previous Bind Datatable
        var BindDataTableP = function (response) {
            var table = $("#tbl_PreRespiratorySystem").DataTable({
                fixedHeader: {
                    header: true,
                    footer: true
                },
                processing: true,
                "deferRender": true,
                "pageLength": 100,
                "retrieve": true,
                lengthChange: true,
                "aaData": response,
                "aoColumns": [
                    {
                        "mData": "resId",
                        "orderable": false,
                        "searchable": false,
                        "className": "text-center",
                        "render": function (data, type, row, meta) {
                            let _index = (meta.row) + 1;
                            if (_index >= 10) {
                                return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>" + _index + "</a>";
                            }
                            else {
                                return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>0" + _index + "</a>";
                            }
                        }
                    },

                    {
                        "mData": "app_fdate",
                        "className": "text-left",
                        "render": function (data) {
                            return moment(data).format('DD-MM-YYYY')
                        }
                    },
                    {
                        "mData": "doctor_name",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_1_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_2_type",
                        "className": 'text-justify text-wrap'

                    },
                    {
                        "mData": "res_3_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_4_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_5_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_6_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_7_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "res_8_type",
                        "className": 'text-justify text-wrap'
                    },
                    {
                        "mData": "resId",
                        "className": 'btn-view text-center',
                        "orderable": false,
                        "searchable": false,
                        "render": function (resId, type, full, meta) {
                            var Action = '';
                            //Copy Data
                            if ("@emp_designation.ToString()" === "Super Administrator") {
                                Action += '<a id=btn_Copy_RespSystem  onclick="CopyPrevious(' + resId + ')"><i class="fa fa-lg fa-copy" ></i></a>';
                                }
                                else {
                                    if ("@emr.set_emr_lock" != "NoLock") {
                                        if ("@emr.app_ae_date" >= "@emr.today") {
                                            Action += '<a id=btn_Copy_RespSystem  onclick="CopyPrevious(' + resId + ')"><i class="fa fa-lg fa-copy" ></i></a>';
                                        }
                                        else {
                                            Action += ''
                                            }
                                    }
                                    else {
                                        Action += '<a id=btn_Copy_RespSystem  onclick="CopyPrevious(' + resId + ')"><i class="fa fa-lg fa-copy" ></i></a>';
                                    }
                                }

                                return Action;
                            }
                    },
                ],
                order: [[0, 'asc']],
                language: {
                    searchPlaceholder: 'Search...',
                    sSearch: '<span class="text-primary font-weight-semi-bold">Search By Code/Tittle</span>',
                    lengthMenu: '_MENU_',
                    scrollX: "100%",
                },
                scrollX: true,
                scrollY: 500,
                scrollCollapse: true,
                buttons: [
                    {
                        extend: 'excel',
                        title: 'Previous Respiratory System',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2]
                        }
                    },
                    {
                        extend: 'pdf',
                        title: 'Previous Respiratory System',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2]
                        }
                    },
                    {
                        extend: 'csv',
                        title: 'Previous Respiratory System',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2]
                        }
                    },
                    {
                        extend: 'print',
                        title: 'Previous Respiratory System',
                        footer: 'true',
                        className: 'oldbutton',
                        exportOptions: {
                            columns: [0, 1, 2]
                        }
                    }
                ]
            });
            table.buttons().container().appendTo('#tbl_PreRespiratorySystem_wrapper .col-md-6:eq(0)');
        };
    //#endregion Previous Bind Datatable

    //#region Copy Previous  RespSystem
        function CopyPrevious(resId) {
            var x = $("#btn_Copy_RespSystem");
                $.ajax({
                type: "GET",
                    url: "@Url.Action("CopyRespiratorySystem", "RespiratorySystem", new { area = "EMR" })?resId=" + resId,
                success: function (response) {
                    if (response.isAuthorized != false) {
                        var _data = response;
                        console.log(response);
                        if (_data.length > 0) {
                            console.log(response[0].res_3_type);
                            $('#ures_1').val(response[0].res_1);
                            $('#ures_2').val(response[0].res_2);
                            $('#ures_3').val(response[0].res_3);
                            $('#ures_4').val(response[0].res_4);
                            $('#ures_5').val(response[0].res_5);
                            $('#ures_6').val(response[0].res_6);
                            $('#ures_7').val(response[0].res_7);
                            $('#ures_8').val(response[0].res_8);
                            $('#ures_1_type').val(response[0].res_1_type);
                            $('#ures_2_type').val(response[0].res_2_type);
                            $('#ures_3_type').val(response[0].res_3_type);
                            $('#ures_4_type').val(response[0].res_4_type);
                            $('#ures_5_type').val(response[0].res_5_type);
                            $('#ures_6_type').val(response[0].res_6_type);
                            $('#ures_7_type').val(response[0].res_7_type);
                            $('#ures_8_type').val(response[0].res_8_type);
                            BindSelect();
                        }

                    }
                    else {
                        console.log("You are not Authorized!");
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion Copy Previous  RespSystem

        // #region Validation Initialization
        var validate = function () {
            $("#form_RespSystem").validate({
                rules: {
                    res_1: {
                        required: true
                    },

                },
                messages: {
                    res_1: {
                        required: "Please Enter Chest!"
                    },

                },
                highlight: function (element) {
