<script>
    //#region Page Load
        $(function () {
            validate();
            if ("@emp_designation.ToString()" === "Super Administrator") {
                $("#btn_Update_LeaveMOH,#btn_Delete_LeaveMOH").show();
            } else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Update_LeaveMOH,#btn_Delete_LeaveMOH").show();

                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Update_LeaveMOH,#btn_Delete_LeaveMOH").show();
                    }
                    else {
                        $("#btn_Update_LeaveMOH,#btn_Delete_LeaveMOH").hide();
                    }
                }
            }
            $('#usl_date1,#usl_date2').bootstrapdatepicker({
                format: "dd-MM-yyyy",
                viewMode: "date",
                todayHighlight: true,
                autoclose: true,
                multidate: false,
                multidateSeparator: "-"
            });

            $("#usl_date1").bootstrapdatepicker("setDate", moment(new Date("@Model.sl_date1")).format("DD-MMMM-YYYY"));
            $("#usl_date2").bootstrapdatepicker("setDate", moment(new Date("@Model.sl_date2")).format("DD-MMMM-YYYY"));
            Get_AllPatientDiagnosis();


        });

    //#endregion Page Load

    //#region Load Get All Diagnosis
    var Get_AllPatientDiagnosis = function () {
        $.ajax({
            type: "GET",
            url: "@Url.Action("GetAllPatientDiagnosis", "ReimbursementForms", new { area = "EMRForms" })?appId=" + "@emr.appId" + "&patId=" + @emr.patId,
            success: function (response) {
                if (response.length > 0) {
                    $('#upat_Diagnosis').text(response[0].Diagnosis);
                }
            },
            error: function (xhr) {
                console.log(xhr);
            }
        })
    }
//#endregion

    //#region Update  Button Click
        $('#btn_Update_LeaveMOH').on('click', function (e) {
             e.preventDefault();
            if ($("#form_EditDentHis").valid()) {
                $('#btn_Update_LeaveMOH').removeClass("btn btn-sm btn-success");
                $('#btn_Update_LeaveMOH').addClass("btn btn-sm btn-success btn-loaders btn-icon");
                $('#btn_Update_LeaveMOH').html("Updating the Record....");

                var _dataInsert = {
                    "slId": @Model.slId,
                    "sl_appId": $("#sl_appId").val(),
                    "sl_words": $("#usl_words").val(),
                    "pat_city": $("#upat_city").val(),
                    "sl_date1": $("#usl_date1").val(),
                    "sl_date2": $("#usl_date2").val(),
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                console.log(_dataInsert)
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateSickLeaveMOH", "SickLeave", new { area = "EMR" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_LeaveMOH').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_LeaveMOH').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_LeaveMOH').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Sick Leave MOH Updated Successfully!", "success");
                                Timeout();
                                GetSickLeaveMOH();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. Sick Leave MOH Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_LeaveMOH').removeClass("btn btn-sm btn-warning btn-loaders btn-icon");
                        $('#btn_Update_LeaveMOH').addClass("btn btn-sm btn-warning");
                        $('#btn_Update_LeaveMOH').html("Save Changes");
                        Timeout();
                    }
                });
            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion Update  Button Click


    //#region Delete Click
        $('#btn_Delete_LeaveMOH').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_LeaveMOH').removeClass("btn btn-sm btn-success");
            $('#btn_Delete_LeaveMOH').addClass("btn btn-sm btn-success btn-loaders btn-icon");
            $('#btn_Delete_LeaveMOH').html("Deleting the Record....");

                $.ajax({
                    url: '@Url.Action("DeleteSickLeaveMOH", "SickLeave", new { area = "EMR" })?slId=' + @Model.slId,
                    type: "POST",
                    dataType: 'JSON',
                    success: function (data) {
                        $('#btn_Delete_LeaveMOH').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_LeaveMOH').addClass("btn btn-sm btn-warning");
                        $('#btn_Delete_LeaveMOH').html("Delete");
                        if (data.isAuthorized) {
                            if (data.success) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Sick Leave MOH Deleted Successfully!", "success");
                                clearControls();
                                GetSickLeaveMOH();
                                Timeout();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_LeaveMOH').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                        $('#btn_Delete_LeaveMOH').addClass("btn btn-sm btn-Delete");
                        $('#btn_Delete_LeaveMOH').html("Delete");
                        Timeout();
                    }
                })
        });
    //#endregion Delete Click

    //#region Validation Initialization
        var validate = function () {

            $("#form_EditDentHis").validate({
                rules: {
                    usl_words: {
                        required: true
                    },

                },
                messages: {
                    usl_words: {
                        required: "Please Other Past History!"
                    },

                },
                highlight: function (element) {
                    var elem = $(element);
                    $(element).parent().addClass('has-error');
                    error_timeout(elem.attr("id"));
                },
                unhighlight: function (element) {
                    var elem = $(element);
                    $(element).parent().removeClass('has-error');
                },
                errorElement: 'span',
                errorClass: 'text-danger fw-bold',
                errorPlacement: function (error, element) {
                    var elem = $(element);
                    if (element.parent('.input-group').length) {
                        error.insertAfter(element.parent());
                    } else {
                        error.insertAfter(element);
                    }
                }
            });

        }
    //#endregion Validation Initialization

    //#region Clear Controls
        var clearControls = function () {
            $(".form-control").val("");
            $(".custom-control-input").prop("checked", false);

        }
