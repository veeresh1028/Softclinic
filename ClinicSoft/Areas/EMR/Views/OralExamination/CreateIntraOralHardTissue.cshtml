        $('#btn_Insert_HardTissue').on('click', function (e) {
            e.preventDefault();
            if ($("#form_HardTissue").valid()) {
                $('#btn_Insert_HardTissue').removeClass("btn btn-sm btn-success");
                $('#btn_Insert_HardTissue').addClass("btn btn-sm btn-success btn-loaders btn-icon");
                $('#btn_Insert_HardTissue').html("Saving the Record....");

                var _hard_tissue_info = {
                    "chtId": 0,
                    "cht_appId": @emr.appId,
                    "cht_1": $("input[name='cht_1']:checked").val(),
                    "cht_2": $("input[name='cht_2']:checked").val(),
                    "cht_3": $("input[name='cht_3']:checked").val(),
                    "cht_4": $("input[name='cht_4']:checked").val(),
                    "cht_5": $("input[name='cht_5']:checked").val(),
                    "cht_6": $("input[name='cht_6']:checked").val(),
                    "cht_7": $("input[name='cht_7']:checked").val(),
                    "cht_8": $("input[name='cht_8']:checked").val(),
                    "cht_9": $("#cht_9").val(),
                    "cht_10": $("#cht_10").val(),
                    "cht_11": $("#cht_11").val(),
                    "cht_12": $("#cht_12").val(),
                    "cht_13": $("#cht_13").val(),
                    "cht_14": $("#cht_14").val(),
                    "cht_15": $("#cht_15").val(),
                    "cht_16": $("#cht_16").val(),
                    "cht_17": $("#cht_17").val(),
                    "cht_18": $("#cht_18").val(),
                }
                var _dateInsert = {
                    hard_tissue_info: _hard_tissue_info,
                    HardTissue_tooth_items: item_arr,
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                console.log(_dateInsert);
                $.ajax({
                        type: "Post",
                        url: "@Url.Action("InsertIntraOralHardTissue", "OralExamination", new {area = "EMR" })",
                        dataType: 'JSON',
                        data: _dateInsert,
                        success: function (data) {
                            $('#btn_Insert_HardTissue').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                            $('#btn_Insert_HardTissue').addClass("btn btn-sm btn-success");
                            $('#btn_Insert_HardTissue').html("Add HardTissue");
                            if (data.isSuccess) {
                                if (data.isInserted) {
                                    var alertPlaceholder = document.getElementById("Alert");
                                    function alert(e, t) {
                                        var l = document.createElement("div");
                                        l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                            '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                    }
                                    alert("<i class='fe fe-check-circle text-white'></i>  IntraOral HardTissue Saved Successfully!", "success");
                                    GetIntraOralHardTissue();
                                    Timeout();
                                }
                                else {

                                    let html = "<div class='col-12 col-sm-12 col-md-6 d-flex justify-content-center'>" +
                                        "<div class='alert alert-warning' role='alert'>" +
                                        "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                        "<strong>Warning.. IntraOral HardTissue Alredy Exists..</strong><br/>" +
                                        "</div></div>";
                                    $("#Alert").html(html);
                                }
                            }
                            else {
                            let html = "<div class='col-12 col-sm-12 col-md-6 d-flex justify-content-center'>" +
                            "<div class='alert alert-danger' role='alert'>" +
                            "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                            "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                        },
                            error: function (xhr) {
                                $('#btn_Insert_HardTissue').removeClass("btn btn-sm btn-success btn-loaders btn-icon");
                                $('#btn_Insert_HardTissue').addClass("btn btn-sm btn-warning");
                                $('#btn_Insert_HardTissue').html("Add HardTissue");
                            Timeout();
                        }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
