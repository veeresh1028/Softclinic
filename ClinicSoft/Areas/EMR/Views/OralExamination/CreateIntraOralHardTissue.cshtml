<script type="text/javascript">
    var item_arr = [];
    //#region Partial View Onload
        $(function () {
            if ("@emp_designation.ToString()" === "Super Administrator") {
                    $("#btn_Insert_HardTissue").show();
            }
            else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Insert_HardTissue").show();

                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Insert_HardTissue").show();
                    }
                    else {
                        $("#btn_Insert_HardTissue").hide();
                    }
                }
            }


            $('#chtd_1').SumoSelect({
                placeholder: 'Select carie(s)',
                captionFormat: '{0} item(s) Selected',
                captionFormatAllSelected: 'All {0} items selected!',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Search here...',
                selectAll: true,
                dropdownParent: $('#form_HardTissue'),
            });

            $("#listItems").hide();
            validate();
            load_Surfaces();

        });
    //#endregion Partial View Onload

    //#region load Surfaces
      var load_Surfaces = function () {
            $('#chtd_3').SumoSelect({
                placeholder: 'Select Surface(s)',
                captionFormat: '{0} Surface(s) Selected',
                captionFormatAllSelected: 'All {0} Surfaces selected!',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Search here...',
                selectAll: true,
                 dropdownParent: $('#form_HardTissue'),
            });



            $.ajax({
                url: '@Url.Action("GetToothSurface", "OralExamination", new { area = "EMR" })?query=',
                type: "GET",
                dataType: "JSON",
                async: false,
                success: function (response) {
                    $('#chtd_3').html('');
                    $('#chtd_3')[0].sumo.reload();

                    $.each(response, function (j) {
                        $('#chtd_3')[0].sumo.add(response[j].id, response[j].text);
                            //$('#select_dept')[0].sumo.selectItem([response[j].id]);
                    });
                    $('#chtd_3')[0].sumo.reload();
                },
                error: function (xhr) {
                    console.log("Failed to Load Branch(es)! Error Message : " + xhr.statusText);
                }
            }).done(function () {
                $('#chtd_3')[0].sumo.reload();
            });
      }
    //#endregion load Surfaces

    //Add Treatment
    $('#btnSelectItem').on('click', function (e) {
        e.preventDefault();
        if ($("#chtd_2").val()!="") {
            var exists = false;

            if (item_arr.length > 0) {
                exists = item_arr.some(function (check) {
                    return check.chtd_2 === $("#chtd_2").val();
                });
            }

            if (exists) {
                var data = $("#hi_pt_tr_name").val();
                data = (data.length >= 75) ? (data.substring(0, 75) + "...") : data;

                Swal.fire({
                    title: "Item Exists!",
                    html: + "<strong style='color:red;'>" + data + "</strong>.<br/> is Already Exists in list Please remove existing one and add again.",
                    icon: "warning",
                    showCancelButton: 0,
                    confirmButtonText: 'OK',
                    confirmButtonClass: "btn btn-success mt-2",
                    buttonsStyling: !1
                })
            }
            else {
                $("#listItems").show();
                bindTreatment();
                clearData();
            }
        }
        else {
            $('.modal-body').animate({
                scrollTop: ($('.has-error').offset().top - 300)
            }, 2000);
        }
    });
    //BindTreatment
    var bindTreatment = function () {


        var data = {
            "chtdId": $("#chtd_2").val(),
            "chtd_appId": @emr.appId,
            "chtd_1": $("#chtd_1").val().toString(),
            "chtd_2": $("#chtd_2").val(),
            "chtd_3": $("#chtd_3 option:selected").text(),
        }


        item_arr.push(data);

        GetToothItems(item_arr);

        $("#btn_Insert_HardTissue").show();
    }
    //Load Tooth Items
        var GetToothItems = function (response) {
            if (response != null && response != undefined) {
                var table;
                console.log(response);
                if ($.fn.dataTable.isDataTable('#dtTable_SelectItems')) {
                    table = $('#dtTable_SelectItems').DataTable();
                    table.clear();
                    table.rows.add(response).draw();

                }
                else {
                    BindDataTableT(response);
                }
            }
        }
     //Bind Treatment DataTable
    var BindDataTableT = function (response) {


        var numberRenderer1 = $.fn.dataTable.render.number(',', '.', 2,).display;

        var table = $("#dtTable_SelectItems").DataTable({
            processing: true,
            paging: false,
            ordering: false,
            info: false,
            "searching": false,
            "retrieve": true,
            lengthChange: false,
            "aaData": response,
            "aoColumns": [
                { "mData": "chtdId" },
                { "mData": "chtd_1" },
                { "mData": "chtd_2" },
                { "mData": "chtd_3" },
                { "mData": "chtdId" },
            ],
            "aoColumnDefs": [
                {
                    "aTargets": [0],
                    "className": "fs-13 text-end",
                    "orderable": false,
                    "searchable": false,
                    "render": function (data, type, row, meta) {
                        let _index = (meta.row) + 1;
                        if (_index >= 10) {
                            return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>" + _index + "</a>";
                        }
                        else {
                            return "<a href='javascript:void(0)' class='fs-13 font-weight-bold'>0" + _index + "</a>";
                        }
                    }
                },
                {
                    "aTargets": [1],
