<script>
    //#region Page Load
        $(function () {
            validate();
            if ("@emp_designation.ToString()" === "Super Administrator") {
                $("#btn_Update_ExtraOralExam,#btn_Delete_ExtraOralExam").show();
            } else {
                if ("@emr.set_emr_lock" == "NoLock") {
                    $("#btn_Update_ExtraOralExam,#btn_Delete_ExtraOralExam").show();

                }
                else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                        $("#btn_Update_ExtraOralExam,#btn_Delete_ExtraOralExam").show();
                    }
                    else {
                        $("#btn_Update_ExtraOralExam,#btn_Delete_ExtraOralExam").hide();
                    }
                }
            }
            var checknames = "@Model.ceo_11";
            if (checknames !== "") {
                GetCheckedValues(checknames);
            } else {
                // Handle the case when pretemp6 is null or empty
            }
           
            
        });

    //#endregion Page Load

    //#region Get CheckedValues
    var GetCheckedValues = function (checknames) {
        var data = checknames;
        if (checknames !== null) {
            var x = data.split(",");

            if (x != "") {
                if (x.includes("1")) {
                    $('#u1').attr('checked', 'checked');
                    $("#u1").prop("checked", true);
                }
                if (x.includes("2")) {
                    $('#u2').attr('checked', 'checked');
                    $("#u2").prop("checked", true);
                }
                if (x.includes("3")) {
                    $('#u3').attr('checked', 'checked');
                    $("#u3").prop("checked", true);
                }
