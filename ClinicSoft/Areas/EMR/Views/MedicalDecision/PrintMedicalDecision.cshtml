                $("#u9").prop("checked", true);
            }
            if (x.includes("10")) {
                $('#u10').attr('checked', 'checked');
                $("#u10").prop("checked", true);
            }
            if (x.includes("11")) {
                $('#u11').attr('checked', 'checked');
                $("#u11").prop("checked", true);
            }
            if (x.includes("12")) {
                $('#u12').attr('checked', 'checked');
                $("#u12").prop("checked", true);
            }
            if (x.includes("13")) {
