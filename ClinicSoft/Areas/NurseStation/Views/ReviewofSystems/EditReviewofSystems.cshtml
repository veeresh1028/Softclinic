<script type="text/javascript">
    //#region Partial View Onload
         $(function () {

             if ("@emp_designation.ToString()" === "Super Administrator") {
                 $("#btn_Update_ROS").show();
                 $("#btn_Delete_ROS").show();
             } else {
                 if ("@emr.set_emr_lock" == "NoLock") {
                     $("#btn_Update_ROS").show();
                     $("#btn_Delete_ROS").show();

                 }
                 else {
                    if ("@emr.app_ae_date" >= "@emr.today") {
                         $("#btn_Update_ROS").show();
                         $("#btn_Delete_ROS").show();
                     }
                     else {
                         $("#btn_Update_ROS").hide();
                         $("#btn_Delete_ROS").hide();
                     }
                 }
             }
             $('#btn_Close_ReviewofSystems').click(function () {
                 $('#ros_add').hide();
             });
             $("#urs_1_type,#urs_2_type,#urs_3_type,#urs_4_type, #urs_5_type, #urs_6_type").select2({
                 placeholder: 'Select Type',
                 width: '100%',
                 minimumResultsForSearch: '',
                 dropdownParent: $('#form_AddROS'),
                 escapeMarkup: function (markup) {
                     return markup;
                 }
             });
             $("#urs_7_type, #urs_8_type, #urs_9_type, #urs_10_type, #urs_11_type, #urs_12_type").select2({
                 placeholder: 'Select Type',
                 width: '100%',
