                 width: '100%',
                 minimumResultsForSearch: '',
                 dropdownParent: $('#form_AddROS'),
                 escapeMarkup: function (markup) {
                     return markup;
                 }
             });
         });
    //#endregion Partial View Onload
    $('#btn_Close_ROS').click(function () {
        $('#ros_edit').hide();
    });
    //#region Clear Controls
        var clearControls = function () {
