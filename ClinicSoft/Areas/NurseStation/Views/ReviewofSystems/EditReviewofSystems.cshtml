    //#endregion Clear Controls

    //#region Update  Button Click
        $('#btn_Update_ROS').on('click', function (e) {
             e.preventDefault();
            if ($("#form_EditROS").valid()) {
                $('#btn_Update_ROS').removeClass("btn btn-success");
                $('#btn_Update_ROS').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Update_ROS').html("Updating the Record....");

                var _dataInsert = {
                    "rsId": $("#ursId").val(),
                    "rs_appId": $("#rs_appId").val(),
                    "rs_1": $("#urs_1").val(),
                    "rs_2": $("#urs_2").val(),
                    "rs_3": $("#urs_3").val(),
                    "rs_4": $("#urs_4").val(),
                    "rs_5": $("#urs_5").val(),
                    "rs_6": $("#urs_6").val(),
                    "rs_7": $("#urs_7").val(),
                    "rs_8": $("#urs_8").val(),
                    "rs_9": $("#urs_9").val(),
                    "rs_10": $("#urs_10").val(),
                    "rs_11": $("#urs_11").val(),
                    "rs_12": $("#urs_12").val(),
                    "rs_13": $("#urs_13").val(),
                    "rs_14": $("#urs_14").val(),
                    "rs_15": $("#urs_15").val(),
                    "rs_16": $("#urs_16").val(),
                    "rs_1_type": $("#urs_1_type").val(),
                    "rs_2_type": $("#urs_2_type").val(),
                    "rs_3_type": $("#urs_3_type").val(),
                    "rs_4_type": $("#urs_4_type").val(),
                    "rs_5_type": $("#urs_5_type").val(),
                    "rs_6_type": $("#urs_6_type").val(),
                    "rs_7_type": $("#urs_7_type").val(),
                    "rs_8_type": $("#urs_8_type").val(),
                    "rs_9_type": $("#urs_9_type").val(),
                    "rs_10_type": $("#urs_10_type").val(),
                    "rs_11_type": $("#urs_11_type").val(),
                    "rs_12_type": $("#urs_12_type").val(),
                    "rs_13_type": $("#urs_13_type").val(),
                    "rs_14_type": $("#urs_14_type").val(),
                    "rs_15_type": $("#urs_15_type").val(),
                    "rs_16_type": $("#urs_16_type").val(),
                    "rs_comments": $("#urs_comments").val(),
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                console.log(_dataInsert)
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateReviewofSystems", "ReviewofSystems", new { area = "NurseStation" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#ros_edit').hide();
                        $('#btn_Update_ROS').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Update_ROS').addClass("btn btn-success");
                        $('#btn_Update_ROS').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> ReviewofSystems Updated Successfully!", "success");
                                Timeout();
                                GetReviewofSystems();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. ReviewofSystems Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_ROS').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Update_ROS').addClass("btn btn-success");
                        $('#btn_Update_ROS').html("Save Changes");
                        Timeout();
                    }
                });
            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion Update  Button Click


    //#region Delete Click
        $('#btn_Delete_ROS').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_ROS').removeClass("btn btn-success");
            $('#btn_Delete_ROS').addClass("btn btn-success btn-loaders btn-icon");
            $('#btn_Delete_ROS').html("Deleting the Record....");

            $.ajax({
                url: '@Url.Action("DeleteReviewofSystems", "ReviewofSystems", new { area = "NurseStation" })?rsId=' + $("#ursId").val(),
                type: "POST",
                dataType: 'JSON',
                success: function (data) {
                    $('#btn_Delete_ROS').removeClass("btn btn-success btn-loaders btn-icon");
                    $('#btn_Delete_ROS').addClass("btn btn-warning");
                    $('#btn_Delete_ROS').html("Delete");
                    if (data.isAuthorized) {
                        if (data.success) {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> Review of Systems Deleted Successfully!", "success");
                            GetReviewofSystems();
                            clearControls();
                            Timeout();
                        }
                        else {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-warning' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                "</div></div>";
                            $("#error").html(html);
                        }
                    }
                    else {
                        var alertPlaceholder = document.getElementById("Alert");
                        function alert(e, t) {
                            var l = document.createElement("div");
                            l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                        }
                        alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                    }
                },
                error: function (xhr) {
                    $('#btn_Delete_ROS').removeClass("btn btn-success btn-loaders btn-icon");
                    $('#btn_Delete_ROS').addClass("btn btn-Delete");
                    $('#btn_Delete_ROS').html("Delete");
                    Timeout();
                }
            })
        });
    //#endregion Delete Click
</script>

