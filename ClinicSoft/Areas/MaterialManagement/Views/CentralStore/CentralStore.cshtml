        //#region Print Item BarCode
        $('#btnPrintBarcode').click(function () {
            var table = $("#tbl_items").DataTable();
            var rows = $(table.$('input[type="checkbox"]').map(function () {
                return $(this).prop("checked") ? $(this).closest('tr') : null;
            }));
            if (rows.length > 0) {
                Swal.fire({
                    title: "Barcode Print",
                    text: "Are you sure you want to Print Selected Item's Barcode",
                    icon: "question",
                    showCancelButton: !0,
                    confirmButtonText: 'Yes! I Confirm',
                    cancelButtonText: 'No! Cancel Please',
                    confirmButtonClass: "btn btn-success mt-2", cancelButtonClass: "btn btn-danger ms-2 mt-2", buttonsStyling: !1
                }).then(function (t) {
                    if (t.value) {
                $.each(rows, function (index, rowId) {
                    var _data = table.row(rowId).data();
                    var SendData = {
                        "ItemId": _data.itemId,
                        "ibId": 0,
                        "ItemCode": _data.item_code,
                        "BranchId": _data.item_branch
                    }
                    $.ajax({
                        url: '@Url.Action("PrintBarcodeService", "CentralStore", new { area = "MaterialManagement" })',
                        type: "POST",
                        dataType: "json",
                        data: SendData,
                        async: false,
                        success: function (response) {
                            if (response.isSuccess == true) {
                                if (response.data["status"] == "success") {
                                    event.preventDefault();
                                    event.stopPropagation();
                                    return $.growl.notice({
                                        title: "Success",
                                        message: response.message
                                    });
                                }
                                else {
                                    event.preventDefault();
                                    event.stopPropagation();
                                    return $.growl.warning({
                                        title: "Pending",
                                        message: response.message
                                    });
                                }
                            }
                            else {
                                event.preventDefault();
                                event.stopPropagation();
                                return $.growl.error({
                                    title: "Error",
                                    message: response.message
                                });
                            }
                        },
                        error: function (xhr) {
                            console.log("Error : " + xhr.statusText);
                        }
                    });
                });
                    }
            });
            }
            else {
                Swal.fire({
                    title: "Alert!",
                    text: "Please select at least one item to print!",
                    icon: "info",
                    showCancelButton: 0,
                    confirmButtonColor: "#0bb197", cancelButtonColor: "#ff3d60",
                });
            }
        });
