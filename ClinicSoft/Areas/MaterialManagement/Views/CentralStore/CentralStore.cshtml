@section myScripts{
    <style>
        .modal-lg {
            max-width: 1500px;
        }

        .dataTables_scrollBody {
            min-height: 300px !important;
        }

        @@media (min-width: 1124px) {
            .dataTables_scrollHeadInner, .table {
                width: 100% !important
            }
        }

        .spinner-grow-sm {
            width: 0.7rem;
            height: 0.7rem;
        }
    </style>

    <script type="text/javascript">
        //#region Page Load
        $(function () {
            load_branches();
            load_status();
            load_groups();
            load_locations();
            load_types();
            $('#select_status')[0].sumo.selectItem(["Active"]);

            BindDataTable(null);
        });
        //#endregion

        //#region Filter on Button Click
        $("#btnSearch").click(function (e) {
            e.preventDefault();

            GetItems();
        });
        //#endregion

        //#region Load Items
        function GetItems() {
            var _data = {
                "item_branch": $('#select_branch').val().toString(),
                "item_type": $('#select_types').val().toString(),
                "item_name_code": $('#text_name_code').val(),
                "item_status": $('#select_status').val().toString(),
                "item_category": $('#select_item_group').val().toString(),
                "item_brand": '',
                "item_location": $('#select_item_location').val().toString()
            }

            $.ajax({
                type: "GET",
                url: "@Url.Action("GetItems", "CentralStore", new { area = "MaterialManagement"})",
                dataType: 'json',
                data: _data,
                beforeSend: function () {
                    $('#progress-loader').show();
                },
                success: function (response) {
                    $('#searchFilter').collapse('hide');

                    if (response.isAuthorized != false) {
                        var table;
                        if ($.fn.dataTable.isDataTable('#tbl_items')) {
                           table = $('#tbl_items').DataTable();
                           table.clear();
                           table.rows.add(response).draw();
                        }
                        else {
                            BindDataTable(response);
                        }
                    }

                    $('#progress-loader').hide();
                },
                error: function (xhr) {
                    console.log(xhr);
                    $('#progress-loader').hide();
                }
            });
        }
        //#endregion

        //#region Bind Datatable
        var BindDataTable = function (response) {
            var table = $("#tbl_items").DataTable({
                fixedHeader: {
                    header: true,
                    footer: true
                },
                processing: true,
                "deferRender": true,
                "pageLength": 100,
                "retrieve": true,
                lengthChange: true,
                scrollX: true,
                scrollY: 500,
                scrollCollapse: true,
                "aaData": response,
                "aoColumns": [
                    {
                        "mData": "itemId",
                        "orderable": false,
                        "searchable": false,
                        "className": "select-checkbox text-center inbox-small-cells",
                        'checkboxes': {
                            'selectRow': true
                        }
                    },
                    {
                        "mData": "itemId",
                        "orderable": false,
                        "searchable": false,
                        "render": function (item_image, type, full, meta) {
                                return '<img class="avatar avatar-xl brround" src=' + full.item_image_path + '></img>';
                        }
                    },
                    {
                        "className": "td_max_width",
                        "mData": "branch_name"
                    },
                    {
                        "mData": "item_code",
                        "className": "font-weight-bold text-info text-wrap",
                        "render": function (data, type, full, meta) {
                            return '<a data-bs-toggle="modal" data-bs-target="#ItemsHistory_modal" onclick="ItemsHistory(\'' + full.itemId + '\',\'' + full.item_code + '\',\'' + full.item_branch + '\')">' + full.item_code + '</a>';
                        }
                    },
                    {
                        "mData": "item_type"
                    },
                    {
                        "mData": "category_name",
                        //"className": "font-weight-bold text-info",
                        //"render": function (data, type, full, meta) {
                        //    return '<a data-bs-toggle="modal" data-bs-target="#StockGroupItems_modal" onclick="ViewGroupsItems(\'' + full.item_category + '\',\'' + full.category_name + '\',\'' + full.item_branch + '\')">' + full.category_name + '</a>';
                            
                        //}
                    },
                    {
                        "mData": "location_name"
                    },
                    {
                        "className": "td_max_width",
                        "mData": "item_name"
                        
                    },
                    {
                        "mData": "item_qty",
                        "sClass": "text-right",
                        "searchable": false,
                        "render": function (item_qty, type, full, meta) {
                            const item_qty_1 = item_qty.toString().match(/^-?\d+(?:\.\d{0,2})?/)[0];
                            if (full.item_qty < full.item_min_qty)
                                return '<i class="spinner-grow spinner-grow-sm text-red" style="animation-duration:1s;" title="Minimum Qty Alter"></i>' + parseFloat(item_qty_1).toLocaleString(undefined, { minimumFractionDigits: 2, maximumFractionDigits: 2 });
                            else
                                return parseFloat(item_qty_1).toLocaleString(undefined, { minimumFractionDigits: 2, maximumFractionDigits: 2 });
                        }
                    },
                    {
                        "mData": "item_uom"
                    },
                    {
                        "mData": "itemId",
                        "className": 'text-center',
                        "render": function (itemId, type, full, meta) {
                            var visible = full.actionvisible;
                            var item_status = full.item_status;
                            if (visible === "True") {
                                if (item_status === "Active") {
                                    return '<button type="button" class="badge rounded-pill bg-success mt-2 border-success" style="width: 60px;" onclick="ChangeStatus(\'' + itemId + '\',\'Active\')">Active</button>'
                                }
                                else if (item_status === "Inactive") {
                                    return '<button type="button" class="badge rounded-pill bg-warning mt-2 border-warning" style="width: 60px;" onclick="ChangeStatus(\'' + itemId + '\',\'Inactive\')">Inactive</button>'
                                }
                                else if (item_status === "Deleted") {
                                    return '<button type="button" class="badge rounded-pill bg-danger mt-2 border-danger" style="width: 60px;" onclick="ChangeStatus(\'' + itemId + '\',\'Deleted\')">Deleted</button>'
                                }
                                else {
                                    return ''
                                }
                            }
                            else {
                                return '<span class="badge rounded-pill text-danger mt-2 border-danger">Allocated</span>'
                            }
                        }
                    },
                    {
                        "className": "text-center",
                        "orderable": false,
                        "searchable": false,
                        "render": function (itemId, type, full, meta) {
                            var status = full.item_status;
                            var visible = full.actionvisible;
                            var html_links = '<div class="dropdown"><a type="button" class="" data-bs-toggle="dropdown"><i class="fe fe-more-vertical" style="font-size: 20px;"></i></a>' +
                                '<div class="dropdown-menu" style="min-width:auto">' +
                                '<a class="dropdown-item text-dark fw-bold" onclick="ViewItemsDetail(' + full.itemId + ')"><i class="fa fa-info-circle text-dark" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Detail</a>';
                            if (status === "Active" && visible === "True") {
                                html_links += '<a class="dropdown-item text-dark fw-bold" onclick="EditItem(' + full.itemId + ')"><i class="fe fe-edit-2 text-success" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Edit</a>';
                                html_links += '<a class="dropdown-item text-dark fw-bold" onclick="UpdateStatus(\'' + full.item_code + '\',\'Deleted\')"><i class="fe fe-trash text-danger" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Delete</a>';
                                html_links += '<a class="dropdown-item text-dark fw-bold" onclick="ItemOpeningStock(\'' + full.itemId + '\',\'' + full.item_code + '\',\'' + full.item_name + '\',\'' + full.item_branch + '\',\'' + full.item_opening_date + '\',\'' + full.item_opening_qty + '\',\'' + full.item_opening_qty2 + '\',\'' + full.item_opening_qty3 + '\',\'' + full.branch_name + '\',\'' + full.item_uom + '\',\'' + full.item_uom2 + '\',\'' + full.item_uom3 + '\',\'' + full.item_m_factor + '\',\'' + full.item_m_factor2 + '\')"><i class="fe fe-package text-dark" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Opening Stock</a>';
                            }
                            if (status === "Active") {
                                html_links += '<a class="dropdown-item text-dark fw-bold" onclick="ItemBatches(\'' + full.item_code + '\',\'' + full.item_branch + '\',\'' + full.item_name + '\',\'' + full.branch_name + '\')"><i class="fe fe-layers text-dark" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Batches</a>';
                            }
                            html_links += '<a class="dropdown-item text-dark fw-bold" onclick= "ItemsAuditLog(\'' + full.item_code + '\')" > <i class="fa fa-eye text-dark" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;Log</a > ';
                            if (status === "Active") {
                                html_links += '<a class="dropdown-item text-dark fw-bold" onclick="ItemsHistory(\'' + full.itemId + '\',\'' + full.item_code + '\',\'' + full.item_branch + '\')"><i class="fa fa-history text-dark" style="font-size: 15px; margin-right: 0.6rem;"></i>&nbsp;History</a>';
                            }
                            html_links += '</div></div>';
                            return html_links;
                        }
                    },

                ],
                select: {
                    style: 'multi',
                    selector: 'td:first-child'
                },
                order:[[7, 'asc']],
                buttons: [
                    {
                        extend: 'excel',
                        title: 'Central Store',
                        footer: 'true',
                        exportOptions: {
                            columns: [2, 3, 4, 5, 6, 7, 8, 9, 10]
                        }
                    },
                    {
                        extend: 'pdf',
                        title: 'Central Store',
                        footer: 'true',
                        exportOptions: {
                            columns: [2, 3, 4, 5, 6, 7, 8, 9, 10]
                        }
                    },
                    {
                        extend: 'csv',
                        title: 'Central Store',
                        footer: 'true',
                        exportOptions: {
                            columns: [2, 3, 4, 5, 6, 7, 8, 9, 10]
                        }
                    },
                    {
                        extend: 'print',
                        title: 'Central Store',
                        footer: 'true',
                        exportOptions: {
                            columns: [2, 3, 4, 5, 6, 7, 8, 9, 10]
                        }
                    }
                ],
                language: {
                    searchPlaceholder: 'Search...',
                    zeroRecords: "No Record(s) Found",
                    sSearch: '<span class="text-primary font-weight-semi-bold">Search By Code / Name / Group / Location / UOM / Type / Status </span>',
                    sEmptyTable: '<span class="text-secondary">No Records Available Based On Request</span>',
                    scrollX: "100%",
                    lengthMenu: '_MENU_',
                }
            });

            table.buttons().container().appendTo('#tbl_items_wrapper .col-md-6:eq(0)');
        }
        //#endregion Bind Datatable

        //#region load branches
        var load_branches = function () {
            $('#select_branch').SumoSelect({
                placeholder: 'Select Branch(es)',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Enter here.',
                selectAll: true
            });

            $('#select_branch').html('');
            $('#select_branch')[0].sumo.reload();

            BranchList =@Html.Raw(Json.Encode(@ViewData["BranchList"]));

            $.each(BranchList, function (j) {
                $('#select_branch')[0].sumo.add(BranchList[j]["Value"], BranchList[j]["Text"]);
                //$('#select_branch')[0].sumo.selectItem(BranchList[j]["Value"]);
            });


            $('#select_branch')[0].sumo.reload();
        };
        //#endregion  load branches

        //#region load Item Groups
        var load_groups = function () {
            $('#select_item_group').SumoSelect({
                placeholder: 'Select Item Group(s)',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Enter here.',
                selectAll: true
            });
            $('#select_item_group').html('');
            $('#select_item_group')[0].sumo.reload();
            ItemGroupList =@Html.Raw(Json.Encode(@ViewData["ItemGroupList"]));
            $.each(ItemGroupList, function (j) {
                $('#select_item_group')[0].sumo.add(ItemGroupList[j]["Value"], ItemGroupList[j]["Text"]);
            });
            $('#select_item_group')[0].sumo.reload();
        };
        //#endregion  load Item Groups

        //#region load Item Location
        var load_locations = function () {
            $('#select_item_location').SumoSelect({
                placeholder: 'Select Item Location(s)',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Enter here.',
                selectAll: true
            });
            $('#select_item_location').html('');
            $('#select_item_location')[0].sumo.reload();
            ItemGroupList =@Html.Raw(Json.Encode(@ViewData["ItemLocationList"]));
            $.each(ItemGroupList, function (j) {
                $('#select_item_location')[0].sumo.add(ItemGroupList[j]["Value"], ItemGroupList[j]["Text"]);
            });
            $('#select_item_location')[0].sumo.reload();
        };
        //#endregion  load Item Location

        //#region load status
        var load_status = function () {
            $('#select_status').SumoSelect({
                placeholder: 'Select Status(es)',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Enter here.',
                selectAll: true
            });

            $('#select_status').html('');
            $('#select_status')[0].sumo.reload();

            $('#select_status')[0].sumo.add("Active", "Active");
            $('#select_status')[0].sumo.add("Inactive", "Inactive");
            $('#select_status')[0].sumo.add("Deleted", "Deleted");

            $('#select_status')[0].sumo.reload();
        };
        //#endregion load status

        //#region load Item Type
        var load_types = function () {
            $('#select_types').SumoSelect({
                placeholder: 'Select Patient Class(es)',
                csvDispCount: 3,
                okCancelInMulti: true,
                isClickAwayOk: true,
                search: true,
                searchText: 'Enter here.',
                selectAll: true
            });

            $('#select_types').html('');
