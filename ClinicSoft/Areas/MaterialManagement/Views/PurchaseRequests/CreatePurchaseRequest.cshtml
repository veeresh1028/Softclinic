    //#region Remove Valiate Error Class Timeout
    function Error_Timeout(_id) {
        setTimeout(function () {
            $(".has-error").removeClass("has-error");
            $(".error").removeClass("errorCreate");
            $("#" + _id + "-error").text("");
        }, 4200);
    }
    //#endregion

    //#region Alert Timeout
    function Timeout() {
        window.setTimeout(function () {
            $(".alert").fadeTo(500, 0).slideUp(500, function () {
                $(this).remove();
            });
        }, 4200);
    }
    //#endregion

    //#region Item Enquiry View
    $('#btnItemEnquiry').on('click', function (e) {
        e.preventDefault();
        $("#spinner2").show();
        $.ajax({
            type: "GET",
            url: "@Url.Action("StockItemEnquiry", "PurchaseOrders", new { area = "MaterialManagement" })",
            contentType: "application/json",
            dataType: "html",
            success: function (data) {

                $('#add_purchaseRequest_modal').modal('hide');
                $('#CentralStoreItems_modal').attr("data-modal-from", "central-store-enquiry-por");

                $("#CentralStoreItems_body").html(data);
                $("#CentralStoreItems_modal").modal("show");

            },
            error: function (xhr) {
                console.log(xhr);
                $("#spinner2").hide();
            },
            complete: function () {
                // Hide spinner when request is complete
                $("#spinner2").hide();
            }
        });
    });
    //#endregion
