@model BusinessEntities.Accounts.MaterialManagement.PurchaseRequests

<div class="modal-header">
    <h5 class="modal-title text-primary font-weight-semibold">Create Purchase Request</h5>
    <button aria-label="Close" data-bs-dismiss="modal" class="btn-close" type="button"><span aria-hidden="true">×</span></button>
</div>

<div class="modal-body p-4">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 mb-4 font-weight-semibold">
            <span class="text-danger" style="font-size:smaller">Required fields are marked with *</span>
        </div>
    </div>

    <form id="form_AddPurchaseRequest" >
        @Html.AntiForgeryToken()

        <div class="row mt-2">
            <div class="panel-group accordion-panel" role="tablist" aria-multiselectable="true">
                <div class="panel panel-default active">
                    <div class="panel-heading bg-danger-1" role="tab" id="required-headingOne1">
                        <h4 class="panel-title">
                            <a role="button" data-bs-toggle="collapse" data-parent="#required-accordion1" href="#required-collapseOne1" aria-expanded="true" aria-controls="required-collapseOne1">
                                <span style="color:white; pointer-events:none;">Purchase Request Detail</span>
                                <span class="float-end" style="color:white;"><i class="fe fe-chevrons-down"></i></span>
                            </a>
                        </h4>
                    </div>
                    <div id="required-collapseOne1" class="panel-collapse collapse show" role="tabpanel" aria-labelledby="required-headingOne1">
                        <div class="mt-5">
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-col-sm-12 col-md-6 col-lg-3">
                                        <div class="form-group">
                                            <label class="form-label text-black">Branch&nbsp;<span class="text-danger">*</span></label>
                                            @Html.DropDownList("purq_branch", (IEnumerable<SelectListItem>)ViewData["BranchList"], "Please Select", new { @class = "form-select select2" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3">
                                        <div class="form-group mb-2">
                                            <label class="form-label text-black">Date&nbsp;<span class="text-danger">*</span></label>
                                        </div>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <i class="fe fe-calendar"></i>
                                                </div>
                                            </div>
                                            @Html.TextBoxFor(sup => sup.purq_odate, new { @class = "form-control", @placeholder = "DD-MMM-YYYY", maxlength = "18" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3">
                                        <div class="form-group">
                                            <label class="form-label text-black">Enq. No#</label>
                                            @Html.TextBoxFor(sup => sup.purq_enqno, new { @class = "form-control mb-4", @placeholder = "Enter Enquiry No.", @style = "width:100%;", maxlength = "20" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Quotation No#</label>
                                            @Html.TextBoxFor(sup => sup.purq_quono, new { @class = "form-control mb-4", @placeholder = "Enter Quotation No.", @style = "width:100%;", maxlength = "20" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Supplier Name&nbsp;<span class="text-danger">*</span></label>
                                            @Html.DropDownListFor(m => m.purq_supplier, new List<SelectListItem>
                                               { }, "Select Supplier", new { @class = "form-control select2" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3">
                                        <div class="form-group">
                                            <label class="form-label text-black">Type&nbsp;<span class="text-danger">*</span></label>
                                            @Html.DropDownListFor(m => m.purq_type, new List<SelectListItem>
                                            {
                                                new SelectListItem{ Text="Purchase Request", Value = "Purchase Request", Selected=true }
                                            }, "Select Type", new { @class = "form-control select2", @disabled = "true" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Quotation No#</label>
                                            @Html.TextBoxFor(sup => sup.purq_quono, new { @class = "form-control mb-4", @placeholder = "Enter Quotation No.", @style = "width:100%;", maxlength = "20" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Validity (Days)</label>
                                            @Html.TextBoxFor(sup => sup.purq_validity, new { @class = "form-control", @placeholder = "Enter Validity of Request", @value = "0", maxlength = "3" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Payment Terms (Days)</label>
                                            @Html.TextBoxFor(sup => sup.purq_pay_terms, new { @class = "form-control", @placeholder = "Enter Pay Terms Days", @value = "0", maxlength = "3" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3 d-none">
                                        <div class="form-group">
                                            <label class="form-label text-black">Quotation Date</label>
                                            @Html.TextBoxFor(sup => sup.purq_qdate, new { @class = "form-control", maxlength = "15", @placeholder = "DD-MMM-YYYY" })
                                        </div>
                                    </div>
                                    <div class="col-col-sm-12 col-md-6 col-lg-3" id="div_sup_invoice">
                                        <div class="form-group">
                                            <label class="form-label text-black">Supplier Invoice&nbsp;<span class="text-danger">*</span></label>
                                            @Html.TextBoxFor(sup => sup.purq_sup_invoice, new { @class = "form-control", maxlength = "15", @placeholder = "Enter Supplier Invoice No." })
                                        </div>
                                    </div>
                                    <div class="col-sm-12 col-md-6 col-lg-6">
                                        <div class="form-group">
                                            <label class="form-label text-black">Notes</label>
                                            @Html.TextAreaFor(sup => sup.purq_notes, new { @class = "form-control", maxlength = "998", @placeholder = "Enter Notes", @rows = "3" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
