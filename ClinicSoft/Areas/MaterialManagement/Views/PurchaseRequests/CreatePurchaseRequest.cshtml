<script type="text/javascript">
    //#region Page Load
    $(function () {
        $('#div_sup_invoice').css('display', 'none');

        $('#purq_branch').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            width: '100%'
        });

        $('#purq_supplier').select2({
            dropdownParent: $('#form_AddPurchaseRequest'),
            placeholder: 'Select Supplier',
            minimumResultsForSearch: -1,
            width: '100%'
        });

        $('#purq_type').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            minimumResultsForSearch: -1,
            width: '100%'
        });

        $('#item_uom_free').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            placeholder: 'Select UoM',
            width: '100%'
        });

        $('#item_uom').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            placeholder: 'Select UoM',
            width: '100%'
        });

        $('#item_disc_type').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            placeholder: 'Select Type',
            width: '100%'
        });

        $('#item_vat_per').select2({
            minimumResultsForSearch: '',
            dropdownParent: $('#form_AddPurchaseRequest'),
            placeholder: 'Select Type',
            width: '100%'
        });

        $('#purq_odate').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
        });

        $('#purq_qdate').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
        });

        $('#item_expiry').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
        });

        $('#item_expiry_free').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
        });

        $("#purq_odate").bootstrapdatepicker("setDate", "today");

        validation();

        $("#btn_Insert").show();

        $("#btnPrintPR").hide();
    });
    //#endregion

    //#region On Branch Selecting Event
    $('#purq_branch').on('select2:selecting', function (e) {
        let branchId = $('#purq_branch').val();

        if (PurchaseRequestItems()) {
            Swal.fire({
                title: 'Do you wish to change the branch?',
                text: 'This action will delete any item(s) which are added in Purchase Request Items!',
                icon: 'question',
                showDenyButton: true,
                confirmButtonText: 'Change',
                confirmButtonClass: 'btn btn-success mt-2',
                denyButtonText: `Don't Change`,
                denyButtonClass: 'btn btn-danger ms-2 mt-2',
                buttonsStyling: !1
            }).then((result) => {
                if (result.isDenied) {
                    e.preventDefault();

