@model BusinessEntities.Accounts.MaterialManagement.PurchaseRequestsAndItems

@{
    ViewBag.Title = "Print Purchase Request";
}
<title>ClinicSoft 9.0 - Powered By Vision Technologies | @ViewBag.Title</title>

