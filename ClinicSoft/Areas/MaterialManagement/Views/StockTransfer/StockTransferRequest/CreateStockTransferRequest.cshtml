    //#region Items Table Validation
    var StockItemsValid = function () {
        let _qty = $('#item_request_qty').val().trim();
        let _uom = $("#item_request_uom").val();
        let _itemQty = $("#hi_request_item_qty").val();
        let _itemQty2 = $("#hi_request_item_qty2").val();
        let _itemQty3 = $("#hi_request_item_qty3").val();
        let _item_uom = $("#hi_request_item_uom").val();
        let _item_uom2 = $("#hi_request_item_uom2").val();
        let _item_uom3 = $("#hi_request_item_uom3").val();
        var count = 0;

        if ($('#item_request_name').val() == undefined || $('#item_request_name').val() == null || $('#item_request_name').val() == '' || $('#item_request_name').val().length == 0) {
            $("#select2-item_request_name-container").parent().addClass('error');
            count++;
            Error_Timeout("item_request_name");
        }

        if ($('#str_from_branch').val() == undefined || $('#str_from_branch').val() == null || $('#str_from_branch').val() == '' || $('#str_from_branch').val().length == 0) {
            $("#select2-str_from_branch-container").parent().addClass('error');
            count++;
            Error_Timeout("str_from_branch");
        }

        if ($('#str_request_branch').val() == undefined || $('#str_request_branch').val() == null || $('#str_request_branch').val() == '' || $('#str_request_branch').val().length == 0) {
            $("#select2-str_request_branch-container").parent().addClass('error');
            count++;
            Error_Timeout("str_request_branch");
        }

        if ($('#str_from_branch').val() === $('#str_request_branch').val()) {
            $("#select2-str_from_branch-container").parent().addClass('error');
            $("#select2-str_request_branch-container").parent().addClass('error');
            count++;
            Error_Timeout("str_from_branch");
            Error_Timeout("str_request_branch");
            $("#lbl_request_branch_error").text("From Branch and To Branch cannot be same.");
        }
        else {
            $("#lbl_request_branch_error").text("");
        }

        if ($('#str_date').val() == undefined || $('#str_date').val() == null || $('#str_date').val() == '' || $('#str_date').val().length == 0) {
            $("#str_date").parent().addClass('has-error');
            count++;
            Error_Timeout("str_date");
        }

        if (_qty === "" || _qty === null || _qty === undefined) {
            $("#lbl_request_calculation_error").text("Qty should be more than 0");
            $("#item_request_qty").attr("style", "border:1px solid red;color:red;");
            count++;
            Error_Timeout("item_request_qty");
        }
        else {
            $("#lbl_request_calculation_error").text("");
            $("#item_request_qty").attr("style", "border:1px solid #ecebf1;color:#728096;");
        }

        if (_uom !== undefined && _uom !== null && _uom !== "" && _uom.length > 0) {
            if (_item_uom === undefined || _item_uom === null || _item_uom === "" ||
                _item_uom2 === undefined || _item_uom2 === null || _item_uom2 === "" ||
                _item_uom3 === undefined || _item_uom3 === null || _item_uom3 === "") {
                $('#item_request_name').parent().addClass('has-error');
                count++;
                Error_Timeout("item_request_name");
                $("#lbl_request_qty").text("Stock Item Not Selected");
            }
            else {
                if (_qty === "" || _qty === null || _qty === undefined) {
                    _qty = 0;
                    $("#item_request_qty").val("0");
                }

                if (_itemQty === "" || _itemQty === null || _itemQty === undefined) {
                    _itemQty = 0;
                    $("#hi_request_item_qty").val("0");
                }

                if (_itemQty2 === "" || _itemQty2 === null || _itemQty2 === undefined) {
                    _itemQty2 = 0;
                    $("#hi_request_item_qty2").val("0");
                }

                if (_itemQty3 === "" || _itemQty3 === null || _itemQty3 === undefined) {
                    _itemQty3 = 0;
                    $("#hi_request_item_qty3").val("0");
                }
            }
        }
        else {
            $('#item_request_uom').parent().addClass('has-error');
            count++;
            Error_Timeout("item_uom");
        }

        if (count == 0) {
            if (_qty <= 0) {
                $("#lbl_request_calculation_error").text("Qty should be more than 0");
                $("#item_request_qty").attr("style", "border:1px solid red;color:red;");
                count++;
                Error_Timeout("item_request_qty");
            }
            else {
                $("#lbl_request_calculation_error").text("");
                $("#item_request_qty").attr("style", "border:1px solid #ecebf1;color:#728096;");
            }
        }

        if ($('#hi_request_item_uom').val() == '' || $('#hi_request_item_uom').val().length == 0 ||
            $('#hi_mfactor').val() == '' || $('#hi_mfactor').val() == 0 ||
            $('#hi_mfactor2').val() == '' || $('#hi_mfactor2').val() == 0 ||
            $('#hi_itemId').val() == '' || $('#hi_itemId').val() == 0
        ) {
            $("#select2-item_request_name-container").parent().addClass('error');           
            count++;
            Error_Timeout("item_request_name");
        }

        if (count > 0)
            return false;
        else
            return true;
    }
    //#endregion

    //#region Validate Stock Transfer Request Items Table
    var ItemsTableValid = function () {
        var count = 0;
        if ($("#tbl_stockTransferRequest_items > tbody > tr").length <= 0) {
            count++;
            $('#tbl_stockTransferRequest_items').parent().addClass('has-error');
            Error_Timeout("tbl_stockTransferRequest_items");
        }
        else {
            var td_text = "";
            $("#tbl_stockTransferRequest_items tbody tr").each(function () {
                var row = $(this);
                td_text = row.find("td").eq(0).html();
            });
            if (td_text.trim() === "No data available in table") {
                count++;
                $('#tbl_stockTransferRequest_items').parent().addClass('has-error');
                Error_Timeout("tbl_stockTransferRequest_items");
            }
        }
        if (count > 0)
            return false;
        else
            return true;
    }
