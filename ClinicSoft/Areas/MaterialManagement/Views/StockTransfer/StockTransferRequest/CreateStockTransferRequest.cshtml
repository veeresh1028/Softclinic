    //#region Insert Stock Transfer Request on Button Click
    $('#btn_Insert').on('click', function (e) {
        e.preventDefault();
        var t = 0;
        if ($('#form_AddTransferRequest').valid() && ItemsTableValid() ) {
            Swal.fire({
                title: "Save",
                text: "Are you sure want to Create Stock Transfer Request!.",
                icon: "question",
                showCancelButton: !0,
                confirmButtonText: 'Yes! I Confirm',
                cancelButtonText: 'No! Cancel Please',
                confirmButtonClass: "btn btn-success mt-2",
                cancelButtonClass: "btn btn-danger ms-2 mt-2",
                buttonsStyling: !1
            }).then(function (t) {
                if (t.value) {
                    if (stockTransferRequest_arr.length > 0) {
                        var transferQty = {
                            stockTransferRequestList: stockTransferRequest_arr,
                            __RequestVerificationToken: $('input[name="__RequestVerificationToken"]').val()
                        }
                        $.ajax({
                            url: '@Url.Action("InsertStockTransferRequest", "StockTransfer", new { area = "MaterialManagement" })',
                            type: "POST",
                            dataType: "json",
                            data: transferQty,
                            beforeSend: function () {
                                $('#btn_Insert').removeClass("btn btn-success");
                                $('#btn_Insert').addClass("btn btn-success btn-loaders btn-icon");
                                $('#btn_Insert').parent().addClass("curdrop");
                                $('#btn_Insert').html("Saving.. Please Wait");
                                $('.btn-close').addClass("disabled");

                            },
                            async: false,
                            success: function (data) {
                                $('#btn_Insert').removeClass("btn btn-success btn-loaders btn-icon");
                                $('#btn_Insert').addClass("btn btn-success");
                                $('#btn_Insert').parent().removeClass("curdrop");
                                $('#btn_Insert').html(" Confirm & Save");
                                $('.btn-close').removeClass("disabled");
                                if (data.isInserted) {

                                    Get_StockTransferRequests($('#str_date').val().trim(), $('#str_date').val().trim());
                                    var alertPlaceholder = document.getElementById("StockTransferRequestAlert");
                                    function alert(e, t) {
                                        var l = document.createElement("div");
                                        l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                            '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                    }
                                    alert("<i class='fe fe-check-circle text-white'></i>Stock Transfer Request Inserted Successfully!", "success");
                                    $('#StockTransferRequest_modal').modal('toggle');
                                    Timeout();

                                }
                                else {
                                    $("#stock_transfer_request_error").html('');
                                    var error_message = data.message;
                                    if (error_message == "Error While Inserting Stock Transfer Request!") {
                                        let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                            "<div class='alert alert-light-warning' role='alert' style=';color:#744f04'>" +
                                            "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                            "<strong><i class='fe fe-alert-triangle'></i>&nbsp;" + error_message + "&nbsp;</strong></div></div>";
                                        $("#stock_transfer_request_error").html(html);
                                    }
                                    else {
                                        let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                            "<div class='alert alert-danger' role='alert'>" +
                                            "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                            "<strong><i class='fe fe-alert-circle text-white' ></i>&nbsp;Validation Error.. Please correct before you Update!&nbsp;</strong><br/><ul>";
                                        $.each(data.message, function (index, value) {
                                            html += "<li><small>" + value + "</small><li>";
                                            var elem = $("#" + index);
                                            if (elem.hasClass("select2-hidden-accessible")) {
                                                $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                            }
                                            else {
                                                $(elem).addClass(" is-invalid");
                                            }
                                            setTimeout(function () {
                                                if (elem.hasClass("select2-hidden-accessible")) {
                                                    $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                                }
                                                else {
                                                    $(elem).removeClass("is-invalid");
                                                }
                                            }, 5000);
                                        });
                                        html += "</ul></div></div>";
                                        $("#stock_transfer_request_error").html(html);
                                    }
                                    Timeout();
                                }
                            },
                            error: function (xhr) {
                                AddingCalculation('Qty1');
                                $('#btn_Insert').removeClass("btn btn-success btn-loaders btn-icon");
                                $('#btn_Insert').addClass("btn btn-success");
                                $('#btn_Insert').parent().removeClass("curdrop");
                                $('#btn_Insert').html(" Confirm & Save");
                                $('.btn-close').removeClass("disabled");
                                var alertPlaceholder = document.getElementById("ConsumptionAlert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                                        '<button type="button" class="btn-close text-white" data-bs-dismiss="alert" aria-hidden="true">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-x-circle text-white'></i> Failed to Create Stock Transfer Request!", "danger");
                                $('#StockTransferRequest_modal').modal('toggle');
                                Timeout();
                            }
                        });
                    }
                }
            });
        }
        else {
            $('.modal-body').animate({
                scrollTop: ($('.has-error').offset().top - 300)
            }, 2000);
            var alertPlaceholder = document.getElementById("stock_transfer_request_error");
            function alert(e, t) {
                var l = document.createElement("div");
                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" role="alert">' + e +
                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button></div>', alertPlaceholder.append(l)
            }
            alert("<i class='fa fa-exclamation-triangle text-warning'></i>Please Make sure to Add Atleast one Items & Fill Mandatory fields!", "warning");
            Timeout();
        }
    });
