                }

                if (_itemQty2 === "" || _itemQty2 === null || _itemQty2 === undefined) {
                    _itemQty2 = 0;
                    $("#hi_request_item_qty2").val("0");
                }

                if (_itemQty3 === "" || _itemQty3 === null || _itemQty3 === undefined) {
                    _itemQty3 = 0;
                    $("#hi_request_item_qty3").val("0");
                }

                //if (_uom === _item_uom) {
                //    if (parseFloat(_qty) > parseFloat(_itemQty)) {
                //        $("#lbl_request_calculation_error").text("Transfer Qty should be less than Stock Qty");
                //        $("#item_request_qty").attr("style", "border:1px solid red;color:red;");
                //    }
                //    else {
                //        $("#lbl_request_calculation_error").text("");
                //        $("#item_request_qty").attr("style", "border:1px solid #ecebf1;color:#728096;");
                //    }
                //}
                //else if (_uom === _item_uom2) {
                //    if (parseFloat(_qty) > parseFloat(_itemQty2)) {
                //        $("#lbl_request_calculation_error").text("Transfer Qty should be less than Stock Qty");
                //        $("#item_request_qty").attr("style", "border:1px solid red;color:red;");
                //    }
                //    else {
                //        $("#lbl_request_calculation_error").text("");
                //        $("#item_request_qty").attr("style", "border:1px solid #ecebf1;color:#728096;");
                //    }
                //}
                //else if (_uom === _item_uom3) {
                //    if (parseFloat(_qty) > parseFloat(_itemQty3)) {
                //        $("#lbl_request_calculation_error").text("Transfer Qty should be less than Stock Qty");
                //        $("#item_request_qty").attr("style", "border:1px solid red;color:red;");
                //    }
                //    else {
                //        $("#lbl_request_calculation_error").text("");
                //        $("#item_request_qty").attr("style", "border:1px solid #ecebf1;color:#728096;");
                //    }
                //}
            }
        }
        else {
            $('#item_request_uom').parent().addClass('has-error');

            Error_Timeout("item_uom");
        }

    }
    //#endregion

    //#region Bind DataTable
    var BindDataTable = function (response) {
        var numberRenderer1 = $.fn.dataTable.render.number(',', '.', 2,).display;
        var table = $("#tbl_stockTransferRequest_items").DataTable({
            processing: true,
            paging: false,
            ordering: false,
            info: false,
            "searching": false,
            "retrieve": true,
            lengthChange: false,
            "aaData": response,
            "aoColumns": [
                {
                    "mData": "str_itemId",
                    "visible": false
                },
                { "mData": "str_item_code" },
                {
                    "mData": "item_request_name",
                    "className": "fs-13 td_max_width"
                },
                {
                    "mData": "str_oqty",
                    "className": "fs-13 text-end",
                    "render": $.fn.dataTable.render.number(',', '.', 2)
                },
                {
                    "mData": "str_ouom",
                    "className":"text-center"
                },
                { "mData": "str_notes" },
                {
                    "mData": "str_itemId",
                    "className": "fs-13 text-center dt-delete",
                    "render": function (data) {
                        var html = "<i class='fe fe-trash text-danger curpointer' id='remove-item' style='font-size: 15px;'></i>";
                        return html;
                    }
                }

            ],
            scrollX: true,
            scrollY: 500,
            scrollCollapse: true,
            fixedColumns: true,
            fixedHeader: {
                header: true,
                footer: true
            },
            footerCallback: function (row, data, start, end, display) {
                var api = this.api();

                // Remove the formatting to get integer data for summation
                var intVal = function (i) {
                    return typeof i === 'string' ? i.replace(/[\$,]/g, '') * 1 : typeof i === 'number' ? i : 0;
                };

                total_qty = api.column(3).data().reduce(function (a, b) { return intVal(a) + intVal(b); }, 0);
                $(api.column(3).footer()).html(numberRenderer1(total_qty.toFixed(2)));

            }
        });
        table.buttons().container()
            .appendTo('#tbl_stockTransferRequest_items_wrapper .col-md-6:eq(0)');

        // Remove Items From DataTable
        table.on('click', '#remove-item', function (e) {
            var row = $(this).parents('tr');
            var data_row = table.row($(this).closest('tr')).data();

            Swal.fire({
                title: "Are you sure you want to delete this Item From List?",
                text: "This action will delete this Item from Selection!",
                icon: "question",
                showCancelButton: !0,
                confirmButtonText: 'Yes! I Confirm',
                cancelButtonText: 'No! Cancel Please',
                confirmButtonClass: "btn btn-success mt-2",
                cancelButtonClass: "btn btn-danger ms-2 mt-2",
                buttonsStyling: !1
