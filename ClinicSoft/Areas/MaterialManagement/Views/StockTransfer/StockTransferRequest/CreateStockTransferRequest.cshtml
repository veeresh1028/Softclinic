

@model BusinessEntities.Accounts.MaterialManagement.StockTransferRequest

<div class="modal-header py-4 px-5">
    <h5 class="modal-title text-primary font-weight-semibold" id="exampleModalLongTitle">Create Stock Transfer Request</h5>
    <button aria-label="Close" data-bs-dismiss="modal" class="btn-close" type="button"><span aria-hidden="true">×</span></button>
</div>

<div class="modal-body p-4">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 mb-4 font-weight-semibold">
            <span class="text-danger" style="font-size:smaller">Required fields are marked with *</span>
        </div>
    </div>

    <form id="form_AddTransferRequest">
        @Html.AntiForgeryToken()
        <div class="row">
            <div class="col-sm-12 col-md-6 col-lg-3">
                <div class="form-group">
                    <label class="form-label text-black">Rquesting Branch&nbsp;<span class="text-danger">*</span></label>
                    @Html.DropDownList("str_request_branch", (IEnumerable<SelectListItem>)ViewData["BranchList"], "Please Select", new { @class = "form-select select2" })
                </div>
            </div>
            <div class="col-sm-12 col-md-6 col-lg-3">
                <div class="form-group">
                    <label class="form-label text-black">From Branch&nbsp;<span class="text-danger">*</span></label>
                    @Html.DropDownList("str_from_branch", (IEnumerable<SelectListItem>)ViewData["BranchList"], "Please Select", new { @class = "form-select select2" })
                </div>
            </div>
            <div class="col-sm-12 col-md-6 col-lg-2">
                <div class="form-group">
                    <label class="form-label text-black">Date&nbsp;<span class="text-danger">*</span></label>
                    @Html.TextBoxFor(sup => sup.str_date, new { @class = "form-control", @placeholder = "DD-MMMM-YYYY", maxlength = "18" })
                </div>
            </div>
        </div>
        <div id="divItems">
            <div class="row" style="border: 1px solid #0e2672; border-width: 1px 0 1px 0px;">
                <div class="mt-5">
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-sm-12 col-md-6 col-lg-4">
                                <div class="form-group">
                                    <label class="form-label text-black">
                                        Items&nbsp;<span class="text-danger">*&nbsp;<label id="lbl_message" class="fw-bold"></label></span>
                                        &nbsp;<a id="btnItemEnquiry" title="Central Store Item Enquiry" style="cursor: pointer"><i class="fa fa-question-circle text-info"></i></a>
                                    </label>
                                    <select class="form-control select2" id="item_request_name">
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-2">
                                <div class="form-group">
                                    <label class="form-label">Qty / UoM&nbsp;<span class="text-danger">*&nbsp;<label id="lbl_request_qty" class="fw-bold"></label></span></label>
                                    <div class="input-group">
                                        <input type="text" id="item_request_qty" class="form-control" maxlength="8" onkeypress="return validateOneDecimalNumeric(this,event);" value="0" />
                                        <div class="input-group-append" style="max-width: 70%; min-width: 50%;">
                                            <select class="form-control select2" id="item_request_uom"></select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-3">
                                <div class="form-group">
                                    <label class="form-label text-black">Items Description</label>
                                    <input type="text" id="item_request_desc" class="form-control" maxlength="495" />
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-2">
                                <div class="form-group">
                                    <label class="form-label text-black">&nbsp;</label>
                                    <button type="button" class="btn btn-secondary" id="btnAddItem" onclick="addStockItemToList();"> <i class="mdi mdi-plus"></i>&nbsp;Add Item</button>

                                    <button type="button" class="btn btn-outline-dark" id="btn_Reset">Reset</button>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4">
                                <div class="form-group">
                                    <label class="form-label text-danger">&nbsp;<span class="text-danger" id="lbl_request_branch_error"></span></label>
                                    <label class="form-label text-danger">&nbsp;<span class="text-danger" id="lbl_request_calculation_error"></span></label>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-12 d-flex justify-content-center" id="exists">
                                <div id="ExistAlert"></div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="table-responsive">
                                <table id="tbl_stockTransferRequest_items" class="table table-striped table-bordered text-nowrap resize-table" style="width: 100%; cursor: pointer;">
                                    <thead>
                                        <tr style="color: #fff !important; background-color: #3C457D; border-color: #3C457D;">
                                            <th class="border-bottom-0 font-weight-semibold text-white">ID</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white">Code</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white">Item</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white">Qty</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white">UoM</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white">Description</th>
                                            <th class="border-bottom-0 font-weight-semibold text-white"></th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                    <tfoot>
                                        <tr>
                                            <td></td>
                                            <td></td>
                                            <td class="text-end fs-14 font-weight-semibold" style="text-align: right !important;"><span class="text-right text-dark fw-bold">Total : &nbsp;</span></td>
                                            <td class="text-end fs-14 font-weight-semibold"></td>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-center">
                    <div id="stock_transfer_request_error" class="text-center" style="max-width:500px"></div>
                </div>
                <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-center">
                    <div id="StockRequestAlert"></div>
                </div>
                @if (TempData["ErrorMessage_Insert"] != null)
                {
                    <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-center">
                        <div class="alert alert-danger alert-dismissible fade show" role="alert">
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                            <strong>Error!&nbsp;</strong>@TempData["ErrorMessage_Insert"]
                        </div>
                    </div>
                }
                <div class="col-sm-12 col-md-12 col-lg-12 text-center">
                    <button type="submit" class="btn btn-success" id="btn_Insert"><i class="fe fe-check-circle"></i>&nbsp;Confirm & Save</button>
                    <button type="button" class="btn btn-outline-danger me-1" data-bs-dismiss="modal">
                        <i class="fe fe-x"></i> Close
                    </button>
                    <label class="text-red" id="request_error_message"></label>
                    <input type="hidden" id="hi_request_item_uom" value="0" />
                    <input type="hidden" id="hi_request_item_uom2" value="0" />
                    <input type="hidden" id="hi_request_item_uom3" value="0" />
                    <input type="hidden" id="hi_mfactor" value="0" />
                    <input type="hidden" id="hi_mfactor2" value="" />
                    <input type="hidden" id="hi_itemId" value="0" />
                    <input type="hidden" id="hi_request_item_qty" value="0" />
                    <input type="hidden" id="hi_request_item_qty2" value="0" />
                    <input type="hidden" id="hi_request_item_qty3" value="0" />
                    <input type="hidden" id="hi_item_request_name" value="" />
                    <input type="hidden" id="hi_item_request_code" value="" />
                    <input type="hidden" id="hi_ib_cost" value="0" />
                    <input type="hidden" id="hi_ib_cost2" value="0" />
                    <input type="hidden" id="hi_ib_cost3" value="0" />
                    <input type="hidden" id="hi_ib_sprice" value="0" />
                    <input type="hidden" id="hi_ib_sprice2" value="0" />
                    <input type="hidden" id="hi_ib_sprice3" value="0" />
                    <input type="hidden" id="hi_item_request_expiry" value="0" />
                </div>
            </div>
        </div>
    </form>
</div>

<script>
    var stockTransferRequest_arr = [];

    //#region Page Load
    $(function () {
        $('#str_request_branch').select2({
            minimumResultsForSearch: '',
            placeholder: 'Select Requesting Branch',
            dropdownParent: $('#form_AddTransferRequest'),
            width: '100%'
        });
        $('#str_from_branch').select2({
            dropdownParent: $('#form_AddTransferRequest'),
            placeholder: 'Select From Branch',
            width: '100%'
        });

        $('#item_request_uom').select2({
            dropdownParent: $('#form_AddTransferRequest'),
            placeholder: 'Select UoM',
            width: '100%'
        });

        // Get the current date
        var currentDate = new Date();
        // Get last year date
        var lastYearDate = new Date(currentDate.getFullYear() - 1, currentDate.getMonth(), currentDate.getDate());

        $('#str_date').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-",
            startDate: lastYearDate,
            endDate: currentDate
        });
        $("#str_date").bootstrapdatepicker("setDate", "today");

        $("#item_request_name").select2({
            placeholder: 'Select a Item',
            width: '100%',
            dropdownParent: $('#form_AddTransferRequest'),
            minimumInputLength: 3,
            escapeMarkup: function (markup) {
                return markup;
            },
            ajax: {
                url: '@Url.Action("SearchItems", "PurchaseOrders", new { area = "MaterialManagement" })',
                dataType: 'json',
                delay: 250,
                data: function (query) {
                    $('#lbl_message').text('');
                    if ($('#str_from_branch').val().trim() != "") {
                        return {
                            query: query.term,
                            branch: $('#str_from_branch').val()
                        };
                    }
                    else {
                        $('#lbl_message').text('Select From Branch First');
                        return {
                            query: '',
                            branch: 0
                        }
                    }
                },
                processResults: function (data) {
                    return {
                        results: data
                    };
                },
                cache: true
            }
        });
        BindDataTable(null);
        validation();
    });
    //#endregion

    //#region From Branch Change Event
    $('#str_from_branch').on('change', function () {
        stockTransferRequest_arr= [];
        GetTransferItems(stockTransferRequest_arr);
        clearData();

    });

    //#endregion

    //#region Adding Validation on Main Form
    var validation = function () {
        $("#form_AddTransferRequest").validate({
            focusInvalid: true,
            rules:
            {
                str_request_branch: {
                    required: true,
                },
                str_from_branch: {
                    required: true,
                },
                str_date: {
                    required: true,
                },
            },
            messages: {
                str_request_branch: {
                    required: "Select From Branch",
                },
                str_from_branch: {
                    required: "Select From Branch",
                },
                str_date: {
                    required: "Select Date",
                },
            },
            highlight: function (element) {
                var elem = $(element);
                if (elem.hasClass("select2-hidden-accessible")) {
                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                } else {
                    elem.parent().addClass('has-error');
                }
                $(element).parent().addClass('has-error');
                Error_Timeout(elem.attr("id"));
            },
            unhighlight: function (element) {
                var elem = $(element);
                if (elem.hasClass("select2-hidden-accessible")) {
                    $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                } else {
                    elem.removeClass('has-error');
                }
                $(element).parent().removeClass('has-error');
            },
            errorElement: 'span',
            errorClass: 'text-danger fw-bold',
            errorPlacement: function (error, element) {
                var elem = $(element);
                if (elem.hasClass("select2-hidden-accessible")) {
                    element = $("#select2-" + elem.attr("id") + "-container").parent();
                    error.insertAfter(element);
                } else {
                    error.insertAfter(element);
                }
                if (element.parent('.input-group').length) {
                    error.insertAfter(element.parent());
                } else {
                    error.insertAfter(element);
                }
            }
        });
    }
    $('#str_date').keypress(function (e) {
        var charCode = (e.which) ? e.which : e.keyCode
        if (String.fromCharCode(charCode).match(/[^0-9./-]/g)) {
            return false;

        }
        else {
            if (e.target.value.length >= 18)
                return false;
        }
    });
    //#endregion

    //#region Validate Text Box with One Decimal and numeric only on Key Press
    function validateOneDecimalNumeric(el, evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode;
        var number = el.value.split('.');
        if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        //just one dot
        if (number.length > 1 && charCode == 46) {
            return false;
        }
        //get the Current position
        var caratPos = getSelectionStart(el);
        var dotPos = el.value.indexOf(".");
        if (caratPos > dotPos && dotPos > -1 && (number[1].length > 1)) {
            return false;
        }
        return true;
    }
    function getSelectionStart(o) {
        if (o.createTextRange) {
            var r = document.selection.createRange().duplicate()
            r.moveEnd('character', o.value.length)
            if (r.text == '') return o.value.length
            return o.value.lastIndexOf(r.text)
        } else return o.selectionStart
    }
    //#endregion

    //#region Item Select Event
    $('#item_request_name').on('select2:select', function (e) {
        var data = e.params.data;
        if (data.id > 0) {
            $.ajax({
                url: '@Url.Action("GetRequestedItemDetail", "StockTransfer", new { area = "MaterialManagement" })?id=' + data.id,
                type: "GET",
                dataType: "json",
                async: false,
                success: function (response) {
                    $("#item_request_uom").empty();
                    if (response.length > 0) {
                        var item_uom = response[0].item_uom;
                        var item_uom2 = response[0].item_uom2;
                        var item_uom3 = response[0].item_uom3;
                        var newOption = "";
                        newOption = new Option(item_uom, item_uom, true, true);
                        $("#item_request_uom").append(newOption).trigger('change');
                        if (item_uom != item_uom2) {
                            newOption = new Option(item_uom2, item_uom2, true, true);
                            $("#item_request_uom").append(newOption).trigger('change');
                        }
                        if (item_uom2 != item_uom3) {
                            newOption = new Option(item_uom3, item_uom3, true, true);
                            $("#item_request_uom").append(newOption).trigger('change');
                        }
                        $('#item_request_uom').val(response[0].item_uom).trigger('change');

                        $('#hi_request_item_qty').val(parseFloat(response[0].item_qty).toFixed(2));
                        $('#hi_request_item_qty2').val(parseFloat(response[0].item_qty2).toFixed(2));
                        $('#hi_request_item_qty3').val(parseFloat(response[0].item_qty3).toFixed(2));
                        $('#hi_request_item_uom').val(response[0].item_uom);
                        $('#hi_request_item_uom2').val(response[0].item_uom2);
                        $('#hi_request_item_uom3').val(response[0].item_uom3);
                        $('#hi_mfactor').val(response[0].item_m_factor);
                        $('#hi_mfactor2').val(response[0].item_m_factor2);
                        $('#hi_itemId').val(response[0].itemId);
                        $('#hi_item_request_name').val(response[0].item_name);
                        $('#hi_item_request_code').val(response[0].item_code);
                        $('#hi_ib_cost').val(response[0].item_cost);
                        $('#hi_ib_cost2').val(response[0].item_cost2);
                        $('#hi_ib_cost3').val(response[0].item_cost3);
                        $('#hi_ib_sprice').val(response[0].item_sprice);
                        $('#hi_ib_sprice2').val(response[0].item_sprice2);
                        $('#hi_ib_sprice3').val(response[0].item_sprice);
                        $('#hi_item_expiry').val('19-April-2050');
                        $('#item_request_qty').val('0');
                    }
                    else {
                        $('#item_request_qty').val('0');
                        $('#item_request_uom').val('-1').trigger('change');
                        $('#hi_request_item_qty').val('0');
                        $('#hi_request_item_qty2').val('0');
                        $('#hi_request_item_qty3').val('0');
                        $('#hi_request_item_uom').val('');
                        $('#hi_request_item_uom2').val('');
                        $('#hi_request_item_uom3').val('');
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                    alert(xhr.statusText);
                }
            }).done(function () {
                //$('#select2-item_request_name-container').attr("title", $('#hi_item_request_name').val());
                setTimeout(function () {
                    $('#select2-item_request_name-container').attr("title", $('#hi_item_request_name').val());
                }, 100);
            });
        }
    });
    //#endregion
    
    //#region Adding Item Calculation
    $("#item_request_qty").focusout('input', function () {
        AddingCalculation();
    });
    $('#item_request_uom').on('select2:select', function (e) {
        AddingCalculation();
    });

    var AddingCalculation = function () {
        $("#lbl_request_calculation_error").text("");
        $("#lbl_request_branch_error").text("");
        $("#lbl_request_qty").text("");
        let _qty = $("#item_request_qty").val();
        let _uom = $("#item_request_uom").val();
        let _itemQty = $("#hi_request_item_qty").val();
        let _itemQty2 = $("#hi_request_item_qty2").val();
        let _itemQty3 = $("#hi_request_item_qty3").val();
        let _item_uom = $("#hi_request_item_uom").val();
        let _item_uom2 = $("#hi_request_item_uom2").val();
        let _item_uom3 = $("#hi_request_item_uom3").val();
        if (_uom !== undefined && _uom !== null && _uom !== "" && _uom.length > 0) {
            if (_item_uom === undefined || _item_uom === null || _item_uom === "" ||
                _item_uom2 === undefined || _item_uom2 === null || _item_uom2 === "" ||
                _item_uom3 === undefined || _item_uom3 === null || _item_uom3 === "") {
                $('#item_request_name').parent().addClass('has-error');
                Error_Timeout("item_request_name");
