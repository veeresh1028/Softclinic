            $("#uchr_etiology").val("");
            $("#uchr_lab").val("");
            $("#uchr_radiology").val("");
            $("#uchr_remarks").val("");
            var elements = document.getElementsByClassName('custom-control-input');
            Array.prototype.forEach.call(elements, function (element) {
                element.checked = false;
