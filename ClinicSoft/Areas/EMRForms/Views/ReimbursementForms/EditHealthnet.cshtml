    //#region Update Button Click
        $('#btn_Update_Healthnet').on('click', function (e) {
            e.preventDefault();
            if ($("#form_EditHealthnet").valid()) {

                $('#btn_Update_Healthnet').removeClass("btn btn-success");
                $('#btn_Update_Healthnet').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Update_Healthnet').html("Updating the Record....");
                var chr_chkbox = "";
                $('.custom-control-input:checked').each(function (index) {
                    chr_chkbox = chr_chkbox + ',' + $(this).val();
                });
                var chr_checkbox = chr_chkbox.substring(1);

                var _dataInsert = {
                "chr_Id": @Model.chr_Id,
                "chr_appId": @emr.appId,
                "chr_checkbox": chr_checkbox,
                "chr_1": $("#uchr_1").val(),
                "chr_2": $("#uchr_2").val(),
                "chr_amount": $("#uchr_amount").val(),
                "chr_symptom": $("#uchr_symptom").val(),
                "chr_condition": $("#uchr_condition").val(),
                "chr_history": $("#uchr_history").val(),
                "chr_etiology": $("#uchr_etiology").val(),
                "chr_lab": $("#uchr_lab").text(),
                "chr_radiology": $("#uchr_radiology").text(),
                "chr_remarks": $("#uchr_remarks").val(),
                "chr_d1": moment($("#uchr_d1").val()).format("YYYY-MM-DD"),
                "chr_d2": moment($("#uchr_d2").val()).format("YYYY-MM-DD"),
                "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateHealthnet", "ReimbursementForms", new { area = "EMRForms" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_Healthnet').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Healthnet').addClass("btn btn-warning");
                        $('#btn_Update_Healthnet').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                GetHealthnet();
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible"  role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Healthnet Updated Successfully!", "success");
                                Timeout();
                                $("#Healthnet_edit").show();
                                $("#Healthnet_add").hide();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. Healthnet Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else
                        {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_Healthnet').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Healthnet').addClass("btn btn-warning");
                        $('#btn_Update_Healthnet').html("Save Changes");
                        Timeout();
                    }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion

    //#region Delete Click
        $('#btn_Delete_Healthnet').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_Healthnet').removeClass("btn btn-success");
            $('#btn_Delete_Healthnet').addClass("btn btn-success btn-loaders btn-icon");
            $('#btn_Delete_Healthnet').html("Deleting the Record....");
                $.ajax({
                    url: '@Url.Action("DeleteHealthnet", "ReimbursementForms", new { area = "EMRForms" })?chr_Id=' + @Model.chr_Id,
                    type: "POST",
                    dataType: 'JSON',
                    success: function (data) {
                        $('#btn_Delete_Healthnet').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Delete_Healthnet').addClass("btn btn-warning");
                        $('#btn_Delete_Healthnet').html("Delete");
                        if (data.isAuthorized) {
                            if (data.success) {
                                GetHealthnet();
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" >' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Healthnet Claim Deleted Successfully!", "success");
                                clearControls();
                                $("#Healthnet_edit").hide();
                                $("#Healthnet_add").show();
                                Addhealthnet();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" >' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_Healthnet').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Delete_Healthnet').addClass("btn btn-Delete");
                        $('#btn_Delete_Healthnet').html("Delete");
                        Timeout();
                    }
                })
        });
