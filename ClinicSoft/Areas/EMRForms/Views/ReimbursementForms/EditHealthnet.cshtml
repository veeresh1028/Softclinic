</div>
<script type="text/javascript">

    //#region Partial View Onload
        var formName1 = $(form_EditHealthnet).closest('form').attr('name');
        $(function () {
            validate();
            set_date();
            Get_EMRINFO();
            GetSignaturename('patient');
            GetSignaturename('Manager');
            GetSignaturename('Authorized');
            GetCheckedValues("@Model.chr_checkbox");
        });
    //#endregion

    //#region Get CheckedValues
        var GetCheckedValues = function (checknames) {
            var str = checknames;
            console.log(str);
            var myarray = str.split(',');
            for (var i = 0; i < myarray.length; i++) {
                $('.custom-control-input').each(function () {
                    if (this.id == myarray[i]) {

                        $("#" + this.id).attr('checked', 'checked');
                        $("#" + this.id).prop("checked", true);
                    }
                });
            }
        }
    //#endregion

    //#region Load set Date
        var set_date = function () {
            $('#uchr_d1,#uchr_d2').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
            });
            var d1 = "@Model.chr_d1";
            if (d1 != "1/1/1950 12:00:00 AM") {
                $("#uchr_d1").bootstrapdatepicker("setDate", moment(new Date("@Model.chr_d1")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#uchr_d1").bootstrapdatepicker("setDate", '');
            }
            var d2 = "@Model.chr_d2";
            if (d2 != "1/1/1950 12:00:00 AM") {
                $("#uchr_d2").bootstrapdatepicker("setDate", moment(new Date("@Model.chr_d2")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#uchr_d2").bootstrapdatepicker("setDate", '');
            }
        }
    //#endregion

    //#region Validation Initialization
        var validate = function () {
            $('#uchr_d1').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                if (String.fromCharCode(charCode).match(/[^0-9./-]/g)) {
                    return false;

                }
                else {
                    if (e.target.value.length >= 18)
                        return false;
                }
            });
            $('#uchr_d2').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                if (String.fromCharCode(charCode).match(/[^0-9./-]/g)) {
                    return false;

                }
                else {
                    if (e.target.value.length >= 18)
                        return false;
                }
            });
            $('#uchr_amount').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                var inputValue = $(this).val() + String.fromCharCode(charCode);

                if (String.fromCharCode(charCode).match(/[^0-9.]/g)) {
                    return false;
                }

                if (parseFloat(inputValue) > 1000000) {
                    return false;
                }
            });
            $("#uchr_amount").on('input', function () {
                var disc = $(this).val();
                if (disc == "" || disc == null || disc == undefined) {
                    $("#uchr_amount").val(0.00);
                }
            });
            $('#uchr_2').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                var inputValue = $(this).val() + String.fromCharCode(charCode);

                if (String.fromCharCode(charCode).match(/[^0-9.]/g)) {
                    return false;
                }

                if (parseFloat(inputValue) > 1000000) {
                    return false;
                }
            });
        }
    //#endregion

