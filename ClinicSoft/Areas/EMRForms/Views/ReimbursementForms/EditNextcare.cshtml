                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <i class="fe fe-calendar"></i>
                                                </div>
                                            </div>
                                            @Html.TextBoxFor(m => m.cncn_date2, new { @class = "form-control", @placeholder = "DD-MMMM-YYYY", @id = "ucncn_date2" })
                                        </div>
                                    </div>
                                </div>
