                                                <div class="input-group-text">
                                                    <i class="fe fe-calendar"></i>
                                                </div>
                                            </div>
                                            @Html.TextBoxFor(m => m.cncn_date1, new { @class = "form-control", @placeholder = "DD-MMMM-YYYY", @id = "ucncn_date1" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row">
