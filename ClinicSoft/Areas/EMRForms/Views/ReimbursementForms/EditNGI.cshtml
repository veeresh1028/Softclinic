    //#region Update Button Click
        $('#btn_Update_NGI').on('click', function (e) {
            e.preventDefault();
            if ($("#form_EditNGI").valid()) {

                $('#btn_Update_NGI').removeClass("btn btn-success");
                $('#btn_Update_NGI').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Update_NGI').html("Updating the Record....");
                var ng_chkbox = "";
                $('.custom-control-input:checked').each(function (index) {
                    ng_chkbox = ng_chkbox + ',' + $(this).val();
                });
                var ng_checkbox = ng_chkbox.substring(1);

                var _dataInsert = {
                "ngId": @Model.ngId,
                "ng_appId": @emr.appId,
                "ng_5": ng_checkbox,
                "ng_1": $("#ung_1").val(),
                "ng_2": $("#ung_2").val(),
                "ng_on": $("#ung_on").val(),
                "ng_eti": $("#ung_eti").val(),
                "ng_no": $("#ung_no").val(),
                "ng_comments": $("#ung_comments").val(),
                "ng_3": moment($("#ung_3").val()).format("YYYY-MM-DD"),
                "ng_4": moment($("#ung_4").val()).format("YYYY-MM-DD"),
                "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateNGI", "ReimbursementForms", new { area = "EMRForms" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_NGI').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_NGI').addClass("btn btn-warning");
                        $('#btn_Update_NGI').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                GetNGI();
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible"  role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> NGI Updated Successfully!", "success");
                                Timeout();
                                $("#NGI_edit").show();
                                $("#NGI_add").hide();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. NGI Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else
                        {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_NGI').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_NGI').addClass("btn btn-warning");
                        $('#btn_Update_NGI').html("Save Changes");
                        Timeout();
                    }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion

