<script type="text/javascript">

    //#region Partial View Onload
        var formName1 = $(form_EditNGI).closest('form').attr('name');
        $(function () {
            validate();
            set_date();
            Get_EMRINFO();
            GetSignaturename('patient');
            GetSignaturename('witness');
            GetCheckedValues("@Model.ng_5");
        });
    //#endregion

    //#region Get CheckedValues
        var GetCheckedValues = function (checknames) {
            var str = checknames;
            console.log(str);
            var myarray = str.split(',');
            for (var i = 0; i < myarray.length; i++) {
                $('.custom-control-input').each(function () {
                    if (this.id == myarray[i]) {

                        $("#" + this.id).attr('checked', 'checked');
                        $("#" + this.id).prop("checked", true);
                    }
                });
            }
        }
    //#endregion

    //#region Load set Date
        var set_date = function () {
            $('#ung_3,#ung_4').bootstrapdatepicker({
            format: "dd-MM-yyyy",
            viewMode: "date",
            todayHighlight: true,
            autoclose: true,
            multidate: false,
            multidateSeparator: "-"
            });
            var d1 = "@Model.ng_3";
            if (d1 != "1/1/1950 12:00:00 AM") {
                $("#ung_3").bootstrapdatepicker("setDate", moment(new Date("@Model.ng_3")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ung_3").bootstrapdatepicker("setDate", '');
            }
            var d2 = "@Model.ng_4";
            if (d2 != "1/1/1950 12:00:00 AM") {
                $("#ung_4").bootstrapdatepicker("setDate", moment(new Date("@Model.ng_4")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ung_4").bootstrapdatepicker("setDate", '');
            }
        }
    //#endregion

    //#region Validation Initialization
        var validate = function () {
            $('#ng_3').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                if (String.fromCharCode(charCode).match(/[^0-9./-]/g)) {
                    return false;

                }
                else {
                    if (e.target.value.length >= 18)
                        return false;
                }
            });
            $('#ng_4').keypress(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode
                if (String.fromCharCode(charCode).match(/[^0-9./-]/g)) {
                    return false;

                }
                else {
                    if (e.target.value.length >= 18)
                        return false;
                }
            });
        }
