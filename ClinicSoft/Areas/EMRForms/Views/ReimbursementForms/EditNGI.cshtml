                        $("#upat_sign2").attr("src", sign);
                        $("#upsb_date_witness").text(date);
                        $("#upsb_date_witness2").text(date);
                    }
                    if (response[0].person == "witness") {
                        $("#uwitimg").show();
                        $("#uwitimg2").show();
                        $("#uwit_sign").attr("src", sign);
                        $("#uwit_sign2").attr("src", sign);
                        $("#upsb_date_witness3").text(date);
                        $("#upsb_date_witness4").text(date);
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
