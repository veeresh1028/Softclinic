    //#region Update Button Click
        $('#btn_Update_Starwell').on('click', function (e) {
            e.preventDefault();
            if ($("#form_EditStarwell").valid()) {

                $('#btn_Update_Starwell').removeClass("btn btn-success");
                $('#btn_Update_Starwell').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Update_Starwell').html("Updating the Record....");
                var sw_chkbox = "";
                $('.custom-control-input:checked').each(function (index) {
                    sw_chkbox = sw_chkbox + ',' + $(this).val();
                });
                var sw_checkbox = sw_chkbox.substring(1);

                var _dataInsert = {
                "swId": @Model.swId,
                "sw_appId": @emr.appId,
                "sw_type": sw_checkbox,
                "sw_pappr": $("#usw_pappr").val(),
                "sw_pamount": parseFloat($("#usw_pamount").val()),
                "sw_comments": $("#usw_comments").val(),
                "sw_pdate": moment($("#usw_pdate").val()).format("YYYY-MM-DD"),
                "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateStarwell", "ReimbursementForms", new { area = "EMRForms" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_Starwell').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Starwell').addClass("btn btn-warning");
                        $('#btn_Update_Starwell').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                GetStarwell();
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible"  role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> Starwell Updated Successfully!", "success");
                                Timeout();
                                $("#Starwell_edit").show();
                                $("#Starwell_add").hide();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. Starwell Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else
                        {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_Starwell').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_Starwell').addClass("btn btn-warning");
                        $('#btn_Update_Starwell').html("Save Changes");
                        Timeout();
                    }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion

    //#region Delete Click
        $('#btn_Delete_Starwell').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_Starwell').removeClass("btn btn-success");
