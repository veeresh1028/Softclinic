            dataType: 'JSON',
            data: _dataInsert,
            success: function (data) {
                $('#btn_Insert_Allianz').removeClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Insert_Allianz').addClass("btn btn-success");
