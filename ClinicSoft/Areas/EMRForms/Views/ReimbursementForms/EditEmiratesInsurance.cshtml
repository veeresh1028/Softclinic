<script type="text/javascript">
    //#region Partial View Onload
        var formName1 = $(form_EditEmiratesInsurance).closest('form').attr('name');
        $(function () {
            validate();
            set_date();
            GetCheckedValues("@Model.cce_chk");
        });
    //#endregion

    //#region Load set Date
        var set_date = function () {
            $('#ucce_date1,#ucce_date2,#ucce_date3,#ucce_date4,#ucce_date5,#ucce_date6,#ucce_date7').bootstrapdatepicker({
                    format: "dd-MM-yyyy",
                    viewMode: "date",
                    todayHighlight: true,
                    autoclose: true,
                    multidate: false,
                    multidateSeparator: "-"
            });
            var d1 = "@Model.cce_date1";
            if (d1 != "1/1/1950 12:00:00 AM") {
                $("#ucce_date1").bootstrapdatepicker("setDate", moment(new Date("@Model.cce_date1")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ucce_date1").bootstrapdatepicker("setDate", '');
            }
            var d2 = "@Model.cce_date2";
            if (d2 != "1/1/1950 12:00:00 AM") {
                $("#ucce_date2").bootstrapdatepicker("setDate", moment(new Date("@Model.cce_date2")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ucce_date2").bootstrapdatepicker("setDate", '');
            }

            var d3 = "@Model.cce_date3";
            if (d3 != "1/1/1950 12:00:00 AM") {
                $("#ucce_date3").bootstrapdatepicker("setDate", moment(new Date("@Model.cce_date3")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ucce_date3").bootstrapdatepicker("setDate", '');
            }
            var d4 = "@Model.cce_date4";
            if (d4 != "1/1/1950 12:00:00 AM") {
                $("#ucce_date4").bootstrapdatepicker("setDate", moment(new Date("@Model.cce_date4")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#ucce_date4").bootstrapdatepicker("setDate", '');
