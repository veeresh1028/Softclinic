                        cce_chkbox = cce_chkbox + ',' + $(this).val();
                    });
                    var cce_checkbox = cce_chkbox.substring(1);
                    var _dataInsert = {
                    "cceId": 0,
                    "cce_appId": @emr.appId,
                    "cce_checkbox": cce_checkbox,
                    "cce_1": $("#cce_1").val(),
