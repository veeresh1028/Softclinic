<script type="text/javascript">

    //#region Partial View Onload
        var formName1 = $(form_EditNASNLGIC).closest('form').attr('name');
        $(function () {
            validate();
            set_date();
            Get_AllPatientDiagnosis();
            GetSignaturename('patient');
        });
    //#endregion

    //#region Load set Date
        var set_date = function () {
            $('#unaln_date1').bootstrapdatepicker({
                    format: "dd-MM-yyyy",
                    viewMode: "date",
                    todayHighlight: true,
                    autoclose: true,
                    multidate: false,
                    multidateSeparator: "-"
            });
            var d1 = "@Model.naln_date1";
            if (d1 != "1/1/1950 12:00:00 AM") {
                $("#unaln_date1").bootstrapdatepicker("setDate", moment(new Date("@Model.naln_date1")).format("DD-MMMM-YYYY"));
            }
            else {
                $("#unaln_date1").bootstrapdatepicker("setDate", '');
            }
        }
    //#endregion

    //#region Load Get All Diagnosis
        var Get_AllPatientDiagnosis = function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllPatientDiagnosis", "ReimbursementForms", new { area = "EMRForms" })?appId=" + "@emr.appId" + "&patId=" + @emr.patId,
                success: function (response) {
                    if (response.length > 0) {
                        $('#unaln_18').text(response[0].Diagnosis);
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            })
        }
    //#endregion

    //#region Update  Button Click
        $('#btn_Update_NASNLGIC').on('click', function (e) {
                e.preventDefault();
            if ($("#form_EditNASNLGIC").valid()) {
                console.log($("#nalnId").val());
                $('#btn_Update_NASNLGIC').removeClass("btn btn-success");
                $('#btn_Update_NASNLGIC').addClass("btn btn-success btn-loaders btn-icon");
                $('#btn_Update_NASNLGIC').html("Updating the Record....");

                var _dataInsert = {
                    "nalnId": @Model.nalnId,
                    "naln_appId": @emr.appId,
                    "naln_1": $("#unaln_1").val(),
                    "naln_2": $("#unaln_2").val(),
                    "naln_3": '',
                    "naln_4": $("#unaln_4").val(),
                    "naln_5": $("#unaln_5").val(),
                    "naln_6": $("#unaln_6").val(),
                    "naln_7": $("#unaln_7").val(),
                    "naln_8": $("#unaln_8").val(),
                    "naln_9": $("#unaln_9").val(),
                    "naln_10": $("#unaln_10").val(),
                    "naln_11": $("#unaln_11").val(),
                    "naln_12": $("#unaln_12").val(),
                    "naln_13": $("#unaln_13").val(),
                    "naln_14": $("#unaln_14").val(),
                    "naln_15": $("#unaln_15").val(),
                    "naln_16": $("#unaln_16").val(),
                    "naln_17": $("#unaln_17").val(),
                    "naln_18": $("#unaln_18").val(),
                    "naln_date1": moment($("#unaln_date1").val()).format("YYYY-MM-DD"),
                    "__RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val()
                }
                console.log(_dataInsert)
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("UpdateNASNLGIC", "ReimbursementForms", new { area = "EMRForms" })",
                    dataType: 'JSON',
                    data: _dataInsert,
                    success: function (data) {
                        $('#btn_Update_NASNLGIC').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_NASNLGIC').addClass("btn btn-warning");
                        $('#btn_Update_NASNLGIC').html("Save Changes");
                        if (data.isSuccess) {
                            if (data.isInserted) {
                                GetNASNLGIC();
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible"  role="alert">' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> NASNLGIC Updated Successfully!", "success");
                                Timeout();
                                $("#nasnlgic_edit").show();
                                $("#nasnlgic_add").hide();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Warning.. NASNLGIC Alredy Exists..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else
                        {
                            let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                "<div class='alert alert-danger' role='alert'>" +
                                "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                "<strong>Validation Error.. Please correct before you submit the form!&nbsp;</strong><br/><ul>";
                            $.each(data.message, function (index, value) {
                                html += "<li><small>" + value + "</small><li>";
                                var elem = $("#" + index);
                                if (elem.hasClass("select2-hidden-accessible")) {
                                    $("#select2-" + elem.attr("id") + "-container").parent().addClass('error');
                                }
                                else {
                                    $(elem).addClass(" is-invalid");
                                }

                                setTimeout(function () {
                                    if (elem.hasClass("select2-hidden-accessible")) {
                                        $("#select2-" + elem.attr("id") + "-container").parent().removeClass('error');
                                    }
                                    else {
                                        $(elem).removeClass("is-invalid");
                                    }
                                }, 5000);
                            });
                            html += "</ul></div></div>";
                            $("#error").html(html);
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Update_NASNLGIC').removeClass("btn btn-warning btn-loaders btn-icon");
                        $('#btn_Update_NASNLGIC').addClass("btn btn-warning");
                        $('#btn_Update_NASNLGIC').html("Save Changes");
                        Timeout();
                    }
                });

            }
            else {
                $('.modal-body').animate({
                    scrollTop: ($('.has-error').offset().top - 300)
                }, 2000);
            }
        });
    //#endregion

    //#region Delete Click
        $('#btn_Delete_NASNLGIC').on('click', function (e) {
            e.preventDefault();
            $('#btn_Delete_NASNLGIC').removeClass("btn btn-success");
            $('#btn_Delete_NASNLGIC').addClass("btn btn-success btn-loaders btn-icon");
            $('#btn_Delete_NASNLGIC').html("Deleting the Record....");

                $.ajax({
                    url: '@Url.Action("DeleteNASNLGIC", "ReimbursementForms", new { area = "EMRForms" })?nalnId=' + @Model.nalnId,
                    type: "POST",
                    dataType: 'JSON',
                    success: function (data) {
                        $('#btn_Delete_NASNLGIC').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Delete_NASNLGIC').addClass("btn btn-warning");
                        $('#btn_Delete_NASNLGIC').html("Delete");
                        if (data.isAuthorized) {
                            if (data.success) {
                                var alertPlaceholder = document.getElementById("Alert");
                                function alert(e, t) {
                                    var l = document.createElement("div");
                                    l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" >' + e +
                                        '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                                }
                                alert("<i class='fe fe-check-circle text-white'></i> NASNLGIC Claim Deleted Successfully!", "success");
                                clearControls();
                                $("#nasnlgic_edit").hide();
                                $("#nasnlgic_add").show();
                                Addnasnlgic();
                            }
                            else {
                                let html = "<div class='col-12 col-sm-12 col-md-12 d-flex justify-content-center'>" +
                                    "<div class='alert alert-warning' role='alert'>" +
                                    "<button type='button' class='btn-close mr-negative-16' data-bs-dismiss='alert' aria-hidden='true' aria-label='Close'>×</button>" +
                                    "<strong>Error!.. Failed to Delete..</strong><br/>" +
                                    "</div></div>";
                                $("#error").html(html);
                            }
                        }
                        else {
                            var alertPlaceholder = document.getElementById("Alert");
                            function alert(e, t) {
                                var l = document.createElement("div");
                                l.innerHTML = '<div class="alert alert-' + t + ' alert-dismissible" >' + e +
                                    '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-hidden="Close">x</button></div>', alertPlaceholder.append(l)
                            }
                            alert("<i class='fe fe-check-circle text-white'></i> You do not have Enough Privileges to Delete this Designation!", "Access Denied!");
                        }
                    },
                    error: function (xhr) {
                        $('#btn_Delete_NASNLGIC').removeClass("btn btn-success btn-loaders btn-icon");
                        $('#btn_Delete_NASNLGIC').addClass("btn btn-Delete");
                        $('#btn_Delete_NASNLGIC').html("Delete");
                        Timeout();
                    }
                })
        });
