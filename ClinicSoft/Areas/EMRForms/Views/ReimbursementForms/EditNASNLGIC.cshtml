                                            <td>@Html.TextBoxFor(m => m.naln_14, new { @class = "form-control", @id = "unaln_14", @maxlength = "10" })</td>
                                        </tr>
                                        <tr>
                                            <td>Country of Treatment</td>
                                            <td colspan="3">@Html.TextBoxFor(m => m.naln_15, new { @class = "form-control", @id = "unaln_15", @maxlength = "100" })</td>
                                        </tr>
                                        <tr>
                                            <td>Total Claimed Amount and Claimed Currency</td>
                                            <td colspan="3">@Html.TextBoxFor(m => m.naln_16, new { @class = "form-control", @id = "unaln_16", @maxlength = "10" })</td>
                                        </tr>
                                    </tbody>
                                </table>
