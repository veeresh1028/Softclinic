    <!-- Title -->
    <title>ClinicSoft 9.0 - Powered By Vision Technologies | Reset Password</title>

    <!--Favicon -->
    <link rel="icon" href="~/assets/images/brand/favicon.ico" type="image/x-icon" />

    <!--Bootstrap css -->
