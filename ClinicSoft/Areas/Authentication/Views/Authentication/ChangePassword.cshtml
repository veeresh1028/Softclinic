<body class="register-3">
    <div class="page">
        <div class="page-content">
            <div class="container">
                <div class="row">
                    <div class="col mx-auto">
                        <div class="row justify-content-center">
                            <div class="col-lg-6 col-xl-4 col-md-7 col-sm-12 col-xs-12 ">
                                <div class="text-center mb-5 mt-0">
                                    <a href="@Url.Action("Login","Authentication", new { area="Authentication" })">
                                        <img src="~/assets/images/brand/logo.png" class="header-brand-img desktop-lgo" alt="ClinicSoft9 logo" style="height: 8rem;">
                                    </a>
                                </div>
                                <div class="card-group mb-0">
                                    <div class="card bg-primary text-white br-7 p-2" style="background-image: linear-gradient(to right, #191a54, #1f354e);">
                                        <div class="card-body mb-0">
                                            <div class="text-center mb-3">
                                                <a href="javascript:void(0);" class="text-white">A Clinic / Medical / Dental / Homecare Center Management Software</a>
                                            </div>
                                            <div class="text-center mb-5">
                                                <a href="javascript:void(0);" class="text-white fs-16 font-weight-semibold">Change Password</a>
                                            </div>
                                            @using (Html.BeginForm())
                                            {
                                                @Html.AntiForgeryToken()
                                                <div class="input-group">
                                                    <div class="input-group-text">
                                                        <i class="fe fe-user"></i>
                                                    </div>
                                                    @Html.EditorFor(model => model.username, new { htmlAttributes = new { @class = "form-control", @placeholder = "Your username", @maxlength = "18" } })
                                                </div>
                                                <div class="form-group mt-2 mb-4">
                                                    @Html.ValidationMessageFor(model => model.username, "", new { @class = "text-danger font-weight-semibold" })
                                                </div>
                                                <div class="input-group mb-4">
                                                    <div class="input-group" id="Password-toggle">
                                                        <a href="" class="input-group-text">
                                                            <i class="fe fe-eye" aria-hidden="true"></i>
                                                        </a>
                                                        @Html.EditorFor(model => model.old_password, new { htmlAttributes = new { @class = "form-control", @placeholder = "Old Password", @maxlength = "12" } })
                                                    </div>
                                                </div>
                                                <div class="form-group mt-2 mb-4">
                                                    @Html.ValidationMessageFor(model => model.old_password, "", new { @class = "text-danger font-weight-semibold" })
                                                </div>
                                                <div class="input-group mb-4">
                                                    <div class="input-group" id="Password-toggle">
                                                        <a href="" class="input-group-text">
                                                            <i class="fe fe-eye" aria-hidden="true"></i>
                                                        </a>
                                                        @Html.EditorFor(model => model.password, new { htmlAttributes = new { @class = "form-control", @placeholder = "New Password", @maxlength = "12" } })
                                                    </div>
                                                </div>
                                                <div class="form-group mt-2 mb-4">
                                                    @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger font-weight-semibold" })
                                                </div>
                                                <div class="input-group mb-4">
                                                    <div class="input-group" id="Password-toggle">
                                                        <a href="" class="input-group-text">
                                                            <i class="fe fe-eye" aria-hidden="true"></i>
                                                        </a>
                                                        @Html.EditorFor(model => model.confirm_password, new { htmlAttributes = new { @class = "form-control", @placeholder = "Confirm Password", @maxlength = "12" } })
                                                    </div>
                                                </div>
                                                <div class="form-group mt-2 mb-4">
                                                    @Html.ValidationMessageFor(model => model.confirm_password, "", new { @class = "text-danger font-weight-semibold" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="custom-control custom-checkbox">
                                                        <span class="text-white-50">&nbsp; <a href="@Url.Action("ForgotPassword","ForgotPassword", new { area="Authentication" })" class="btn-link text-white float-end">Forgot password ?</a></span>
                                                    </label>
                                                </div>
                                                <div class="row">
                                                    <div class="col-12">
                                                        <div id="errorMsg" class="text-danger text-center">
                                                            @Html.ValidationSummary(true, "", new { @class = "text-danger font-weight-semibold fs-14" })
                                                        </div>
                                                        <input type="submit" value="Change Password" class="btn btn-white btn-block text-primary d-grid px-4 font-weight-bold" />
                                                    </div>
                                                </div>
                                                <div class="form-group fs-12 text-center mt-4">
                                                    By clicking "Change Password" and setting a new password, you agree to our <a href="javascript:void(0);" style="color: #49abc9" class="font-weight-bold">Terms &amp; Conditions</a> acknowledge that you have read and understood our <a href="javascript:void(0);" style="color: #49abc9" class="font-weight-bold">Privacy &amp; Services.</a> policies.
                                                </div>
                                            }
                                            <div class="text-center">
                                                <div class="font-weight-normal fs-14 text-white-80"><i class="fe fe-arrow-left"></i> <a class="btn-link font-weight-bold anchors text-white" href="@Url.Action("Login","Authentication", new { area="Authentication" })">Back to Login</a></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="text-center register-icons mt-2">
                                    <div class="small text-dark font-weight-bold mb-4">
                                        <p>
                                            &copy; @DateTime.Now.Year - <a href="https://visionsoftwares.com" target="_blank" class="small text-dark font-weight-bold mb-4">Vision Technologies</a>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery js-->
    <script src="~/assets/js/jquery.min.js"></script>

    <!-- Bootstrap5 js-->
    <script src="~/assets/plugins/bootstrap/popper.min.js"></script>
    <script src="~/assets/plugins/bootstrap/js/bootstrap.min.js"></script>

    <!--Othercharts js-->
    <script src="~/assets/plugins/othercharts/jquery.sparkline.min.js"></script>

    <!-- Circle-progress js-->
    <script src="~/assets/js/circle-progress.min.js"></script>

    <!-- Jquery-rating js-->
    <script src="~/assets/plugins/rating/jquery.rating-stars.js"></script>

    <!-- Show Password -->
    <script src="~/assets/js/bootstrap-show-password.min.js"></script>

    <!-- Custom js-->
    <script src="~/assets/js/custom.js"></script>

    @Scripts.Render("~/bundles/jqueryval")

</body>
