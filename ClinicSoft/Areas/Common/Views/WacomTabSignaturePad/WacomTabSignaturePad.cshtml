@model BusinessEntities.Common.eSignature


<div class="modal-header py-3 px-4">
    <h5 class="modal-title text-primary font-weight-semibold" id="exampleModalLongTitle">S</h5>
    <button type="button" class="btn-close close-modal" data-bs-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body p-4">
    <div id="div_signcontract">
        <div class="row">
            <div class="col-md-4">
                <div id="imageBox" class="boxed" style="height: 35mm; width: 60mm; border: 1px solid #d3d3d3;" ondblclick="displaySignatureDetails(sigObj)" title="Double-click a signature to display its details">
                </div>
            </div>
            <div class="col-md-4">
                <div>
                    <input type="button" value="Capture" style="height: 10mm; width: 35mm" onclick="capture()"
                           title="Starts signature capture" />
                </div>
                <div>
                    <input type="button" value="Clear" style="height: 10mm; width: 35mm" onclick="clearSignature()"
                           title="Clears the signature" />
                </div>
                <div>
                    <input type="button" id="Restore" value="Restore" disabled style="height: 10mm; width: 35mm" onclick="setSignatureText()"
                           title="Restores the signature from the SigText data. To use this function please tick <Output SigText to form>" />
                </div>
            </div>
            <div class="col-md-4">
                <div>
                    <input type="button" value="Verify" style="height: 10mm; width: 35mm" onclick="verifySignedData()"
                           title="Checks the signature hash" />
                </div>
                <div>
                    <input type="button" value="License Info" style="height: 10mm; width: 35mm" onclick="aboutBox(sigCtl)"
                           title="Displays the Help About box" />
                </div>
                <div>
                    <input type="button" value="Signature Details" style="height: 10mm; width: 35mm" onclick="displaySignatureDetails(sigObj)"
                           title="Displays the Help About box" />
                </div>
            </div>
        </div>
        <div class="row">
            <span>Data included in the hash:</span>
        </div>
         <div class="row">
             <div class="col-md-2">First name:</div>
             <div class="col-md-2">  <input type="text" id="fname" value="John" /></div>
             <div class="col-md-2">Last name:</div>
             <div class="col-md-2"><input type="text" id="lname" value="Smith" /></div>
        </div>
         <div class="row">
             <h3>Options</h3>
        </div>
         <div class="row">
             <div class="col-md-6"><input type="checkbox" id="chkUseB64Image" runat="server" checked="checked" />Use base-64 signature image</div>
             <div class="col-md-6"><input type="checkbox" id="chkShowSigText" onclick="enableRestoreButton()" runat="server" />Output SigText to form</div>  
         </div>  
         <div class="row">
             <div class="col-md-12">@Html.TextAreaFor(m => m.txtDisplay, new { @class = "form-control", @row = "2" })</div>
         </div>
         <div class="row">
             <div class="col-md-1">Signature:</div>
             <div class="col-md-11">@Html.TextAreaFor(m => m.txtSignature, new { @class = "form-control", @row = "2" })</div>
         </div>     
       
        <div>
            <input id="btnSubmitSign" type="button" value="Submit Sign" onclick="fun_submit()" />
            <input id="btnClearSign" type="button" value="Clear" onclick="init_Sign_Canvas()" />
        </div>
    </div>
</div>

<style type="text/css">
    #div_signcontract {
        width: 99%;
    }

    .popupHeader {
        margin: 10px;
    }
</style>
<script src="~/Wacom_sign/wgssSigCaptX.js"></script>
<script src="~/Wacom_sign/base64.js"></script>
<script src="~/Wacom_sign/SigCaptX-Utils.js"></script>
<script src="~/Wacom_sign/SigCaptX-SessionControl.js"></script>
<script src="~/Wacom_sign/SigCaptX-Globals.js"></script>

<script type="text/javascript">
    var isSign = false;
    var leftMButtonDown = false;

    $(function () {
        //Initialize sign pad
        //init_Sign_Canvas();
    });

    function fun_submit() {       

        var _data = {
            "patId": @Model.patId,
            "person": "@Model.person",
            "form": "@Model.form",
            "appId": @Model.appId,
            "formname": "@Model.formname",
            "formlink": "@Model.formlink",
            "eSign": $("#txtDisplay").val(),
            "txtSignature": $("#txtDisplay").val()
        }
        //var _data = {
        //    "patId": 2382,
        //    "person": "patient",
        //    "form": "GeneralConsent",
        //    "appId": 4065,
        //    "formname": "General Consent",
        //    "formlink": "General ConsentPrint",
        //    "eSign": "Signature SDK Service detected.\nChecking components ...\nDLL: flSigCOM.dll  v4.5.1.205\nDLL: flSigCapt.dll v4.5.1.205\nTest application ready.\nPress 'Capture' or 'Start Wizard' to capture a signature.\nCreating hash:\nSignature captured successfully\nbase64_image:><\n",
        //    "txtSignature": "Signature SDK Service detected.\nChecking components ...\nDLL: flSigCOM.dll  v4.5.1.205\nDLL: flSigCapt.dll v4.5.1.205\nTest application ready.\nPress 'Capture' or 'Start Wizard' to capture a signature.\nCreating hash:\nSignature captured successfully\nbase64_image:><\n"
        //}

        console.log(_data);
        $.ajax({
            type: "Post",
            url: "@Url.Action("UploadSignature", "WacomTabSignaturePad", new {area = "Common" })",
            dataType: 'JSON',
