        $('#uapp_patient').on('change', function () {
            $(this).trigger('blur');
        });
        $('#uapp_type').on('change', function () {
            $(this).trigger('blur');
        });
        $('#uapp_status').on('change', function () {
            $(this).trigger('blur');
        });
