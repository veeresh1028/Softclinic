    //#region Insert Insurance Company Click
    $('#btn_Update').on('click', function (e) {
        e.preventDefault();

        if ($("#form_Update_Appointment").valid()) {
            var _appData = {
                "appId"                 : (@Model.appId),
                "app_room"              : $("#uapp_room").val(),
                "app_fdate"             : moment($("#uapp_fdate").val()).format("YYYY-MM-DD"),
                "app_tdate"             : moment($("#uapp_fdate").val()).format("YYYY-MM-DD"),
                "app_ftime"             : $("#uapp_ftime").val(),
                "app_ttime"             : $("#uapp_ttime").val(),
                "app_ins"               : $("#uapp_patient").val(),
                "app_doctor"            : $("#uapp_doctor").val(),
                "app_type"              : $("#uapp_type").val(),
                "app_status"            : $("#uapp_status").val(),
                "app_branch"            : $("#uapp_branch").val(),
                "app_inout"             : $("#uapp_inout").val(),
                "app_emergency"         : $("#uapp_emergency").val(),
                "app_eligibility"       : $("#uapp_eligibility").val(),
                "app_reason_for_visit"  : $("#uapp_reason_for_visit").val(),
                "app_package_order"     : $("#uapp_package_order").val(),
                "app_comments"          : $("#uapp_comments").val(),
                "app_service"           : $("#uapp_service").val(),
            }

            $.ajax({
                type: "POST",
                url: "@Url.Action("EditAppointment", "RoomScheduler", new { area = "Appointment" })",
                dataType: 'JSON',
                data: _appData,
                success: function (data) {
                    let _id = parseInt(data);
                    var _toast_html = "";

                    if (_id !== undefined && _id !== "" && _id !== null) {
                        if (_id === -1) {
                            _toast_html = toast_html('info', 'Appointment', '<i class="fe fe-alert-triangle me-2 text-warning"></i> Appointment already exist with same details...');
                        }
                        else if (_id > 0) {
                            _toast_html = toast_html('success', 'Appointment', '<i class="fe fe-check-circle me-2 text-success"></i> You have successfully updated the appointment...');
                            Calendar.init();
                        }
                        else {
                            _toast_html = toast_html('error', 'Appointment', '<i class="fe fe-slash me-2 text-danger"></i> Failed To Update The Appointment!');
                        }
                    }
                    else {
                        _toast_html = toast_html('error', 'Appointment', '<i class="fe fe-slash me-2 text-danger"></i> Failed To Update The Appointment!');
                    }

                    $('#update_appointment_modal').modal('toggle');
                    var calenderToast = document.getElementById('calenderToast');
                    calenderToast.innerHTML = _toast_html;
                    var toast = new bootstrap.Toast(calenderToast)
                    toast.show();
                },
                error: function (xhr) {
                    $('#update_appointment_modal').modal('toggle');

                    var _toast_html = toast_html('error', 'Appointment', 'Error in updating the appointment...');
                    var calenderToast = document.getElementById('calenderToast');
                    calenderToast.innerHTML = _toast_html;
                    var toast = new bootstrap.Toast(calenderToast)
                    toast.show();
                }
            });
        }
        else {
            $('.modal-body').animate({
                scrollTop: ($('.has-error').offset().top - 300)
            }, 2000);
        }
    });
    //#endregion

    //#region Clone / Re-book / Delete Appointment
    $("#btnClone").click(function () {
        localStorage.setItem("clone_appointment", @Model.appId);
        $('#update_appointment_modal').modal('toggle');
    });

    $("#btnRebook").click(function () {
        localStorage.setItem("copy_appointment", @Model.appId);
        $('#update_appointment_modal').modal('toggle');
    });

    $("#btnDelete").click(function () {
        Swal.fire({
            title: "Are you sure you want to Delete this appointment ?",
            text: "This action will Delete this Appointment!",
            icon: "question",
            showCancelButton: !0,
            confirmButtonText: 'Yes! I confirm',
            cancelButtonText: 'No! Cancel Please',
            confirmButtonClass: "btn btn-success mt-2",
            cancelButtonClass: "btn btn-danger ms-2 mt-2",
            buttonsStyling: !1
        }).then(function (t) {
            if (t.value) {
                $.ajax({
                    type: "GET",
                    url: "@Url.Action("DeleteAppointment", "RoomScheduler", new { area = "Appointment" })?appId="+ @Model.appId,
                    contentType: "application/json",
                    dataType: "json",
                    success: function (response) {
                        if (parseInt(response) > 0) {
                            $('#update_appointment_modal').modal('toggle');
                            Calendar.init();

                            var _toast_html = "";
                            _toast_html = toast_html('success', 'Appointment', 'You have successfully Deleted the appointment...');
                            var calenderToast = document.getElementById('calenderToast');
                            calenderToast.innerHTML = _toast_html;
                            var toast = new bootstrap.Toast(calenderToast)
                            toast.show();
                        }
                        else {
                            var _toast_html = "";
                            _toast_html = toast_html('error', 'Appointment', 'Error in Deleting the Appointment...');
                            var calenderToast = document.getElementById('calenderToast');
                            calenderToast.innerHTML = _toast_html;
                            var toast = new bootstrap.Toast(calenderToast)
                            toast.show();
                        }
                    },
                    error: function (xhr) {
                        console.log(xhr);
                        alert(xhr);
                    }
                });
            }
        });
    });
    //#endregion

    //#region Toast Body
    var toast_html = function (type, heading, content) {
        var div = "";

        if (type === "success") {
            div = '<div class="toast-header bg-success text-white">';
        }
        else if (type === "error") {
            div = '<div class="toast-header bg-danger text-white">';
        }
        else if (type === "info") {
            div = '<div class="toast-header bg-info text-white">';
        }
        else if (type === "warning") {
            div = '<div class="toast-header bg-warning text-white">';
        }
        else {
            div = '<div class="toast-header bg-primary text-white">';
        }

        var html = div +
            '<strong class="me-auto">' + heading +'</strong>' +
            '<small>just now</small>' +
            '<button type="button" class="btn-close btn-close-white" data-bs-dismiss="toast" aria-label="Close">' +
            '<span aria-hidden="true">×</span>' +
            '</button>' +
            '</div>' +
            '<div class="toast-body">' +
            content
            '</div>';

        return html;
    }
