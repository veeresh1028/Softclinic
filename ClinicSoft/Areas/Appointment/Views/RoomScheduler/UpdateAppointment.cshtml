    //#region Set Patient Details
    var setPatient = function (app_ins) {
        $.ajax({
            url: '@Url.Action("SearchPatients", "RoomScheduler",  new { area = "Appointment" })?query=' + app_ins +'&type=5',
            type: "GET",
            dataType: "json",
            async: false,
            success: function (response) {
                var data = {
                    "id": response[0].id,
                    "text": response[0].text,

                };

                var newOption = new Option(response[0].text, response[0].id, true, true);
                $('#uapp_patient').append(newOption).trigger('change');

                $('#uapp_patient').trigger({
                    type: 'select2:select',
                    params: {
                        data: data
                    }
                });

            },
            error: function (xhr) {
                console.log(xhr);
                alert(xhr);
            }
        });
