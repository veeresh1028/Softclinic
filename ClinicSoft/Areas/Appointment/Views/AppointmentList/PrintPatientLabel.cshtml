@model BusinessEntities.Patient.PatientLabel
<div style="font-size:14px;font-weight:600">
    @Html.DisplayFor(model => model.pat_code) <br />
    @Html.DisplayFor(model => model.pat_name) <br />
    @Html.DisplayFor(model => model.pat_dob, "{0:dd/MM/yyyy}") / @Html.DisplayFor(model => model.pat_gender) <br />
    @Html.DisplayFor(model => model.pat_mob) <br />
    @Html.DisplayFor(model => model.pat_ins) / @Html.DisplayFor(model => model.nationality)
</div>